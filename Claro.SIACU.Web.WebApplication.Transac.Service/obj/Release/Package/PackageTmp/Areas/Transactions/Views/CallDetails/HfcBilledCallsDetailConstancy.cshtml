@{
    ViewBag.Title = "HfcBilledCallsDetailConstancy";
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="HfcBilledCallsDetailConstancy" class="container">
    <header>
        <nav class="navbar btn-danger navbar-fixed-top">
            <div class="container">
                <div style="text-align: center;">
                    <h4><strong><span id="spnMainTitle">CONSTANCIA DETALLE DE LLAMADA SALIENTE - HFC - FACTURADA</span></strong></h4>
                </div>
            </div>
        </nav>
    </header>
    <div class="row">
        <div class="container">
            <div class="col-md-12">
                <br />
                <center>
                    <span><strong>Solicitud de Variación de Servicio - Detalle de Llamadas</strong></span>
                    <img alt="Claro" src="~/images/logo-claro.png">
                </center>
                <div class="panel panel-danger">
                    <div class="panel-body">
                        <table id="tbDatoCliente" class="table table-striped  table-responsive">
                            <tr>
                                <td>
                                    <label class="control-label">Centro Atencion:</label>
                                </td>
                                <td>
                                    <span id="lblCACDAC"></span>
                                </td>
                                <td>
                                    <label><b>&#124;</b></label>
                                </td>

                                <td>
                                    <label class="control-label">Fecha (dd/mm/aaaa):</label>
                                </td>
                                <td>
                                    <span id="lblFecha"></span>
                                </td>
                            </tr>
                            <tr>
                                <td> <label class="control-label">Titular:</label></td>
                                <td><span id="lblTitular"></span></td>
                                <td>
                                    <label><b>&#124;</b></label>
                                </td>
                                <td><label class="control-label">Caso / Interacción:</label></td>
                                <td><span id="lblCaso"></span></td>

                            </tr>
                            <tr>
                                <td> <label class="control-label">Representante Legal:</label></td>
                                <td><span id="lblRepresentante"></span></td>
                                <td>
                                    <label><b>&#124;</b></label>
                                </td>
                                <td><label class="control-label">Nro. Claro:</label></td>
                                <td><span id="lblNroClaro"></span></td>

                            </tr>
                            <tr>
                                <td> <label class="control-label">Tipo Doc. Identidad:</label></td>
                                <td><span id="lblTipoDoc"></span></td>
                                <td>
                                    <label><b>&#124;</b></label>
                                </td>
                                <td><label class="control-label">Contrato:</label></td>
                                <td><span id="lblContrato"></span></td>

                            </tr>
                            <tr>
                                <td> <label class="control-label">Nro. Doc:</label></td>
                                <td><span id="lblNroDoc"></span></td>
                                <td>
                                    <label><b>&#124;</b></label>
                                </td>
                                <td><label class="control-label">Customer ID:</label></td>
                                <td><span id="lblCustomerId"></span></td>

                            </tr>
                        </table>

                    </div>

                </div>

                <div class="panel panel-danger">
                    <div class="panel-heading">
                        Meses Solicitados:
                    </div>
                    <div class="panel-body">
                        <table id="" class="table table-condensed ">
                            <tr>
                                <td>
                                    <label class="control-label">Meses Solicitados: </label>
                                </td>
                                <td>
                                    <span id="lblMesesSolicitado"></span>
                                </td>
                            </tr>
                            <tr>
                                <td> <label class="control-label">Correo Electrónico: </label></td>
                                <td><span id="lblCorreoDL"></span></td>
                            </tr>
                            <tr>
                                <td> <label class="control-label">Medio: </label></td>
                                <td>
                                        Correo Electrónico:<input type="checkbox" id="chkCorreoDL" name="chkCorreoDL" disabled="disabled">
                                </td>
                            </tr>
                        </table>

                    </div>

                </div>
                <div class="panel panel-danger">
                    <div class="panel-heading">
                        Notas
                    </div>
                    <div class="panel-body">
                        <form id="tbDatoTransaccion3" class="form-horizontal">
                            <div style="float: left; width: 95%; height: auto; margin-top: 4px;">
                                * Sólo podrá solicitar el detalle de llamadas como máximo de los dos últimos ciclos de facturación y sólo sobre el número de días que el abonado figure como titular de la línea.
                            </div>
                            <div style="float: left; margin-top: 10px; color: Navy; font-size: 10px; text-align: justify; width: 98%; padding: 1%;">
                                "Declaro que soy responsable por la custodia y reserva de la información que sea enviada a través de dicho correo electrónico, el cual ha sido exprésamente solicitada por mi. Por lo expuesto, libero a América Móvil Perú S.A.C. de cualquier responsabilidad por el envío, reserva y custodia de la información solicitada y remitida por los medios antes indicados y, renuncio expresa y anticipadamente a cualquier reclamo que de ella derive, inclusive a reclamos por pago de indemnizaciones de cualquier tipo."
                            </div>
                            <div style="float: left; margin-top: 10px; border: solid; border-width: 1px; font-size: 10px; border-color: Black; color: Navy; text-align: justify; width: 98%; padding: 1%;">
                                Por la presente doy fe que los datos personales antes consignados son verdaderos y autorizo a América Móvil Perú SAC, que en caso alguno de los datos proporcionados no sean válidos o no coincidan con los registros oficiales, se dé por inválida esta solicitud.
                            </div>
                        </form>
                    </div>
                </div>

                <div class="panel panel-danger">
                    <div class="panel-body">
                        <form id="tbDatoTransaccion4" class="form-horizontal">

                            <div style="float:left; width:90px; height:100px;">
                                <!-- en blanco-->
                            </div>

                            <div style="float:left; width:120px; height:100px;">
                                <div style="height:70px; border-bottom-style:solid; border-bottom-color:Black;"></div>
                                <div style="text-align: center; padding-top: 5px;">
                                    <strong>Firma del Cliente</strong>
                                </div>
                            </div>

                            <div style="float:left; width:80px; height:100px;">
                                <!-- en blanco central-->
                            </div>
                            <div style="float: left; width: 120px; height: 100px;">
                                <div style="height:70px; border-bottom-style:solid; border-bottom-color:Black;"></div>
                                <div style="text-align: center; padding-top: 5px;">
                                    <strong>Firma del Asesor</strong>
                                </div>
                            </div>
                            <div style="float: left; width: 120px; height: 100px;">
                                <!-- en blanco-->
                            </div>
                        </form>
                    </div>
                </div>


                <div>
                    <center>
                        <form class="form-inline ">
                            <div class="form-group">
                                <button class="btn claro-btn-info btn-sm" onclick="printDiv('ContenedorPinPuk')" id="btnPrint" type="button"><span class="glyphicon glyphicon-print">&nbsp;</span>Imprimir</button>
                                <button class="btn claro-btn-info btn-sm" onclick="Cerrar()" id="btnCerrar" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                            </div>

                        </form>
                    </center>
                </div>
                <div id="myModalLoad" style="display: none;">
                    <span>
                        <img src="~/Images/loading2.gif" width="25" height="25" /> Cargando ....
                    </span>
                </div>
            </div>
        </div>
        <br /><br /><br />
    </div>
</div>

@section scripts
{
    <script type="text/javascript" src="~/Areas/Transactions/Scripts/CallDetails/HFCBilledCallsDetailConstancy.js"></script>
}

<script>
    function printDiv(divName) {
        window.print();
    }
    function Cerrar() {
        this.close();
    }

</script>