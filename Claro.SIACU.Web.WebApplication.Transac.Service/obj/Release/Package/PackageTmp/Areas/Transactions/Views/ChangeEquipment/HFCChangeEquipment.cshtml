@{
    Layout = null;
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>

<div id="HFCChangeEquipment" class="container-fluid">


<div class="scrollbudy">
<div class="col-md-3">
    <div id="DataCustomer" class="panel-group" role="tablist" aria-multiselectable="true">

        <div class="panel claro-panel-primary">
            <div id="customerDataHead" role="tab" class="panel-heading">
                <span class="glyphicon glyphicon-user"></span>
                <a data-toggle="collapse" href="#DatosClientecollapse" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Datos del Cliente</a>
            </div>
            <div id="DatosClientecollapse" class="panel-body panel-collapse collapse in" role="tabpanel" aria-labelledby="customerDataHead">
                <table class="table table-condensed">
                    <tr>
                        <td>Contrato</td>
                        <td><label id="lblIdContract"></label></td>
                    </tr>
                    <tr>
                        <td>Customer ID</td>
                        <td><label id="lblCustomerID"></label></td>
                    </tr>
                    <tr>
                        <td>Tipo de Cliente</td>
                        <td><label id="lbltypeCustomer"></label></td>
                    </tr>

                    <tr>
                        <td>Cliente</td>
                        <td><label id="lblCustomerName"></label></td>
                    </tr>

                    <tr>
                        <td>Contacto</td>
                        <td><label id="lblContact"></label></td>
                    </tr>

                    <tr>
                        <td>DNI/RUC</td>
                        <td><label id="lblIdentificationDocument"></label></td>
                    </tr>

                    <tr>
                        <td>Representante Legal</td>
                        <td><label id="lblRepLegal"></label></td>
                    </tr>

                    <tr>
                        <td>Plan Actual</td>
                        <td><label id="lblPlanActual"></label></td>
                    </tr>

                    <tr>
                        <td>Fecha de Activación</td>
                        <td><label id="lblDateAct"></label></td>
                    </tr>

                    <tr>
                        <td>Ciclo de Facturación</td>
                        <td><label id="lblCycleBilling"></label></td>
                    </tr>

                    <tr>
                        <td>Límite de Crédito<br />(S/ inc. IGV)</td>
                        <td><label id="lblBillingAmount"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Estado</td>
                        <td><label id="lblStatus"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Fecha Vcto.</td>
                        <td><label id="lblDateVcto"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Cuenta</td>
                        <td><label id="lblCuenta"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Cod. Edificio</td>
                        <td><label id="CodeBuilding"></label></td>
                    </tr>

                </table>
            </div>
            <div id="addressDataHead" role="tab" class="panel-heading">
                <span class="glyphicon glyphicon-home"></span>
                <a data-toggle="collapse" href="#addressDataCollapse" aria-expanded="false" data-parent="#DataCustomer" aria-controls="addressDataCollapse" class="collapsed transaction-ref-color"> Dirección de Facturación</a>
            </div>
            <div class="panel-body panel-collapse collapse " id="addressDataCollapse" role="tabpanel" aria-labelledby="addressDataHead">
                <table class="table table-condensed">
                    <tr>
                        <td>Dirección</td>
                        <td><label id="lblAddress"></label></td>
                    </tr>
                    <tr>
                        <td>Referencia</td>
                        <td><label id="lblAddressNote"></label></td>
                    </tr>
                    <tr>
                        <td>País</td>
                        <td><label id="lblCountry"></label></td>
                    </tr>
                    <tr>
                        <td>Departamento</td>
                        <td><label id="lblDepartament"></label></td>
                    </tr>
                    <tr>
                        <td>Provincia</td>
                        <td><label id="lblProvince"></label></td>
                    </tr>
                    <tr>
                        <td>Distrito</td>
                        <td><label id="lblDistrict"></label></td>
                    </tr>
                    <tr>
                        <td>Código Plano</td>
                        <td><label id="lblCodePlane"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Código Ubigeo</td>
                        <td><label id="lblCodeUbigeo"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>HUB</td>
                        <td><label id="lblHUB"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>Cintillo</td>
                        <td><label id="lblBelt"></label></td>
                    </tr>
                    <tr style="display:none">
                        <td>CMTS</td>
                        <td><label id="lblCMTS"></label></td>
                    </tr>
                </table>
            </div>

            <div id="BussinessRuleHead" role="tab" class="panel-heading">
                <span class="glyphicon glyphicon-rub"></span>
                <a data-toggle="collapse" href="#BussinessRule" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color">&nbsp;  Reglas de Atención</a>
            </div>
            <div id="BussinessRule" class="panel-body panel-collapse collapse" role="tabpanel" aria-labelledby="customerDataHead">

            </div>

        </div>


    </div>

</div>
<div id="content" class="col-md-9 transaction-content">

<div id="divChangeEquip" class="tab-pane active">
    <div id="tabChangeEquipment" class="step tab-pane active" role="tabpanel" percent="16%">
        <div class="panel claro-panel-primary">

            <div class="panel-body">
                <form class="form-horizontal">
                    <div class="panel claro-panel-primary">
                        <div id="divChangeEquipment" role="tab" class="panel-heading">
                            <span class="fa fa-table"></span>
                            <label data-toggle="collapse" href="#" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Detalle de Productos</label>
                        </div>

                        <div class="panel-body">

                            <div id="IDdivtabProductsCE"></div>

                        </div>
                    </div>

                    <div id="IDdivtabTypeWorkCE"></div>
                    <center>
                        <button id="btnSave" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                        <button id="btnClose" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                        <button id="btnConstancy" type="button" class="btn transaction-button" style="display: none;"><span class="glyphicon glyphicon-list-alt"></span> Constancia</button>
                    </center>
                </form>
            </div>

            <div id="divErrorCE"></div>

        </div>
    </div>
</div>






</div>
</div>


<div id="divErrorAlert" class="alert alert-transaction alert-danger" role="alert" style="display:none">
    <button type="button" id="closeErrorAlert" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <strong><span id="lblErrorMessage"></span></strong>
</div>

<div id="IDdivtabTypeWork" class="panel claro-panel-primary" style="display: none">
    <div role="tab" class="panel-heading">
        <span class="fa fa-cogs"></span>
        <label data-toggle="collapse" href="#" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Tipo Trabajo</label>
    </div>
    <div class="panel-body">
        <form class="form-horizontal">

            <div class="form-group">
                <label for="ddlTypeWork" class="col-sm-2 input-sm control-label">Tipo de Trabajo</label>
                <div class="col-sm-4">
                    <select class="form-control input-sm" id="ddlTypeWork" disabled></select>
                </div>

                <label for="ddlReasonSot" class="col-sm-2 control-label input-sm ">Motivo SOT</label>
                <div class="col-sm-4">
                    <select class="form-control input-sm" id="ddlReasonSot"></select>
                </div>
            </div>

            <div id="tr_SubWorkType" class="form-group">
                <label for="ddlTypeSubWork" class="col-sm-2 control-label input-sm ">Sub Tipo de Trabajo (*)</label>
                <div class="col-sm-4">
                    <select class="form-control input-sm" id="ddlTypeSubWork"></select>
                </div>
            </div>

            @Html.Partial("~/Areas/Transactions/Views/Shared/ViewPartial/Schedule.cshtml",
                new ViewDataDictionary {
                    { "Text_Id", "txtFProgramacion" },
                    { "Select_Id", "cboFranjaHoraria" },
                    { "Button_Id", "btnValidateSchedule" },
                    { "Combo_JobTypes" , "ddlTypeWork"},
                    { "Combo_SubJobType" , "ddlTypeSubWork"},
                    { "Input_Size" , "input-xs"},
                    { "strDateServer", @ViewData["strDateServer"] },
                    { "strDateNew", @ViewData["strDateNew"] },
                    { "Button_Save", "btn_saveTransaction"}
                })

            @*<div class="form-group">
                <label class="col-sm-2 control-label" style="float: left">
                    Fidelizar
                    <input class="checkbox-inline" type="checkbox" id="chkLoyalty" value="true">
                </label>

                <label class="col-sm-4 control-label" style="float: left">
                    Costo de Instalación (Con IGV) S/ :
                </label>
                <div class="col-sm-2">
                    <input type="text" readonly="readonly" class="form-control input-sm" id="txChargeAmount" aria-describedby="helpBlock2">
                </div>


                <div class="col-sm-6 input-sm " id="divlnknumsot" style="display: none;">
                   
                </div>

            </div>*@
            <div class="form-group">
                <label class="col-sm-2 control-label input-sm " style="float: left">
                    E-mail
                    <input class="checkbox-inline input-sm" type="checkbox" id="chkSendMail" value="true">
                </label>

                <div class="col-sm-4" id="dContenedorEmail">
                    <input type="email" id="txtSendMail" class="form-control" placeholder="Correo Electrónico" style="display: none;">
                    <span id="ErrorMessageEmail" class="alert-danger"></span>
                </div>
            </div>
            <div class="form-group">
                <label for="ddlCACDAC" class="col-sm-2 control-label input-sm "> Punto de Atención (*)</label>
                <div class="col-sm-4">
                    <select class="form-control input-sm" id="ddlCACDAC"></select>
                </div>
            </div>
            <div class="form-group">
                <label for="txtNote" class="col-sm-2 control-label input-sm ">Notas</label>
                <div class="col-sm-9">
                    <textarea class="form-control input-sm" style="resize: none" rows="6" placeholder="Agregar Notas" id="txtNote"></textarea>
                </div>
            </div>

        </form>
    </div>
</div>
@*dataTables_wrapper form-inline dt-bootstrap4 no-footer*@
<div id="IDdivtabProducts" style="display:none">
    <table id="tblExtransf" class="table table-bordered table-hover table-striped">
        <thead class="btn-danger">
        <tr>
            <th style="width:100px">Cod. Material</th>
            <th style="width:100px">Cod. SAP</th>
            <th style="width:100px">Num. Serie</th>
            <th style="width:100px">Mac Ad.</th>
            <th style="width:100px">Desc. Material</th>
            <th style="width:100px">Tipo Equipo</th>
            <th style="width:100px">Código / Id Producto</th>
            <th style="width:100px">Modelo</th>
            <th style="width:100px">Conv. Type</th>
            <th style="width:100px">Serv/Paque. Principal</th>
            <th style="width:100px">Head End</th>
            <th style="width:100px">Perfil</th>
            <th style="width:100px">Número</th>
        </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

</div>

<input id="hidOpcion" type="hidden" name="hidOpcion" />
<input id="hidCodOpcion" type="hidden" name="hidCodOpcion" />
<input id="hidAccion" type="hidden" name="hidAccion" />


@Scripts.Render("~/bundles/steps")
@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
<script src="~/Areas/Transactions/Scripts/ReadRecordPdf/ReadRecord.js"></script>
<script src="~/Areas/Transactions/Scripts/ChangeEquipment/HFCChangeEquipment.js"></script>
