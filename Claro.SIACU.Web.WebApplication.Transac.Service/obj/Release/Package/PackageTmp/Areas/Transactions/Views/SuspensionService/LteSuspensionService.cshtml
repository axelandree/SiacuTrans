@{
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div class="container">
    <div class="row">
        <div class="col-md-3">
            <div id="PostpaidDataCustomer" class="panel claro-panel-primary ">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-book"></span>
                    <a data-toggle="collapse" href="#DatosContratoCollapse" style="color: #fff">
                        Datos del Contrato
                    </a>
                </div>
                <div id="DatosContratoCollapse">
                    <table class="table table-striped">
                        <tr>
                            <td>Contrato</td>
                            <td><label id="lblContract" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>Tipo de Cliente</td>
                            <td><label id="lblTypeCustomer" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>Contacto</td>
                            <td><label id="lblContactName" class="text-message-info"></label></td>
                        </tr>
                    </table>
                </div>
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span>
                    <a data-toggle="collapse" href="#DatosClientecollapse" style="color: #fff">
                        Datos del Cliente
                    </a>
                </div>
                <div id="DatosClientecollapse">
                    <table class="table table-striped">
                        <tr>
                            <td>Cliente</td>
                            <td><label id="lblCustomerName" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>DNI/RUC</td>
                            <td><label id="lblDniRuc" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>Fecha de Activación</td>
                            <td><label id="lblDateActivation" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>Representante Legal</td>
                            <td><label id="lblLegalRepresentative" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td>DNI Repres. Legal</td>
                            <td><label id="lblDniLegalRepres" class="text-message-info"></label></td>
                        </tr>
                    </table>
                </div>
                <div id="businessRulesHead" role="tab" class="panel-heading">
                    <span class="glyphicon glyphicon-th-list"></span>
                    <a data-toggle="collapse" href="#businessRulesCollapse" aria-expanded="false" data-parent="#DataCustomer" aria-controls="businessRulesCollapse" class="collapsed transaction-ref-color"> Reglas de Atención</a>
                </div>
                <div class="panel-body panel-collapse collapse in" id="businessRulesCollapse" role="tabpanel" aria-labelledby="businessRulesHead">
                    <div id="divRules">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-9">
            <div id="" class="panel claro-panel-primary">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-book"></span>
                    <a data-toggle="collapse" href="#Contratcollapse" style="color: #fff">
                        Datos de la Transacción
                    </a>
                </div>
                <div id="Contratcollapse" class="">
                    <div id="tbDatoTransaccion" class="form-horizontal">
                        <br />
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Fecha Suspensión:</label>
                            <div class="col-sm-3">
                                <div class="input-group">
                                    <input type="text" id="txtSuspensionDate" class="form-control" />
                                    <label class="input-group-addon btn" for="txtSuspensionDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Fecha Reactivación:</label>
                            <div class="col-sm-3">
                                <div class="input-group">
                                    <input type="text" id="txtReactivationDate" class="form-control" />
                                    <label class="input-group-addon btn" for="txtReactivationDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Importe a pagar:</label>
                            <div class="col-sm-3">
                                <input type="text" id="txtAmountPay" class="form-control" value="" />
                            </div>
                            <div id="ContainerCheckRetention">
                                <label class="col-sm-2 control-label">Por retención</label>
                                <div class="col-sm-1 checkbox">
                                    <label>
                                        <input type="checkbox" id="chkRetention" style="position: relative; top: 3px;">
                                    </label>
                                </div>

                            </div>
                        </div>

                        <div class="form-group" id="idtdMontoRet">
                            <label class="col-sm-3 control-label">Monto Retención:</label>
                            <div class="col-sm-3">
                                <input type="text" id="txtAmountRetention" class="form-control" />
                            </div>

                            <label class="col-sm-2 control-label">Total a pagar: </label>
                            <div class="col-sm-3">
                                <input type="text" id="txtTotalPay" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Punto de atención: </label>
                            <div class="col-sm-3">
                                <select id="ddlCACDAC" class="form-control"></select>
                            </div>
                        </div>
                        <div class="form-group" id="idHdnSendEmail">
                            <label class="col-sm-3 control-label input-sm">Enviar Correo: <input type="checkbox" id="chkSentEmail"></label>
                            <div class="col-sm-3">
                                <input style="display: none" id="txtSendforEmail" type="text" class="form-control" placeholder="Email" name="txtSendforEmail">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Notas:</label>
                            <div class="col-sm-6">
                                <textarea id="txtNotes" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div>
                <center>
                    <div class="form-inline ">
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnSave" type="button"><span class="glyphicon glyphicon-floppy-disk">&nbsp;</span>Guardar</button>
                        </div>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnClose" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                        </div>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnConstancy" type="button"><span class="glyphicon glyphicon-print">&nbsp;</span>Exportar</button>
                        </div>
                    </div>
                </center>
            </div>
        </div>
    </div>
</div>

<div id="myModalLoad" style="display: none;">
    <span>
        <img src="~/Images/loading2.gif" width="25" height="25" /> Cargando ....
    </span>
</div>


@section scripts
{
    <script type="text/javascript" src="~/Areas/Transactions/Scripts/SuspensionService/LteSuspensionService.js"></script>
}









