@using Claro.SIACU.Web.WebApplication.Transac.Service.Areas.Transactions.Models;

@{
    //Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    Layout = null;
}
<style>
    .panel {
        margin-bottom:10px !important;
    }
</style>
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="RecordEquipmentContainer" class="contenedor">    
    <input type="hidden" id="hidInfoTraz" />
    <input type="hidden" value='@Model.strTipoDocumentos' id="hidTipoDocumentos" />
    <input type="hidden" value='@Model.strCodigoTipoDocumentoDNIValidacionBiometrica' id="hidCodigoTipoDocumentoDNIValidacionBiometrica" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometrica1' id="hidMensajeValidacionBiometrica1" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometrica2' id="hidMensajeValidacionBiometrica2" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometrica3' id="hidMensajeValidacionBiometrica3" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometrica4' id="hidMensajeValidacionBiometrica4" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometricaMenos2' id="hidMensajeValidacionBiometricaMenos2" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometricaMenos4' id="hidMensajeValidacionBiometricaMenos4" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometricaMenos5' id="hidMensajeValidacionBiometricaMenos5" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometrica0' id="hidMensajeValidacionBiometrica0" />
    <input type="hidden" value='@Model.strMensajeValidacionBiometricaOtros' id="hidMensajeValidacionBiometricaOtros" />
    <input type="hidden" value='@Model.strKeyTransaccionDesbloqueoLinea' id="hidKeyTransaccionDesbloqueoLinea" />
    <input type="hidden" value='@Model.strMensajeBlackList' id="hidstrMensajeBlackList" />
    <input type="hidden" value='@Model.strFlagValidaBlackList' id="hidstrFlagValidaBlackList" />
    <input type="hidden" value='@Model.strFlagPermisoBiometria' id="hidstrFlagPermisoBiometria" />

    <input type="hidden" value='@ViewData["keyTranEquipoExtranjeroPre"]' id="hdnstrTranEquipoExtranjeroPre" />
    <input type="hidden" value='@ViewData["keyTranEquipoExtranjeroPost"]' id="hdnstrTranEquipoExtranjeroPost" />
    <div class="scrollbudy">
        <div class="col-md-12">
            
                <div class="col-md-12">
                    <div id="obtenerDatosCliente1" class="panel panel-default col-md-12">
                        <table class="table table-condensed">
                            <tbody>
                                <tr>
                                    <td class="col-md-4"><label>Nro. Teléfono</label>&nbsp;:&nbsp;<b><span id="lblPhoneNumber" /></b></td>
                                    <td class="col-md-4"><label>Tipo Cliente</label>&nbsp;:&nbsp;<b> <span id="lblCustomerType" /></b></td>
                                    <td class="col-md-4"><label>Contacto</label>&nbsp;:&nbsp;<b><span id="lblContact" /></b></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="col-md-12">
                    <div id="obtenerDatosCliente" class="panel panel-default col-md-12">
                        <table class="table table-condensed">
                            <thead>
                                <tr>
                                    <th>Datos del Cliente</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="col-md-6"><label>Cliente :</label>&nbsp;&nbsp;<span id="lblCustomer"></span></td>
                                    <td class="col-md-6"><label>Representante Legal :</label>&nbsp;&nbsp;<span id="lblLegalAgent"></span></td>
                                </tr>
                                <tr>
                                    <td class="col-md-6"><label>Tipo de Documento :</label>&nbsp;&nbsp;<span id="lblDocumentType"></span></td>
                                    <td class="col-md-6"><label>Tipo de Documento RRLL :</label>&nbsp;&nbsp;<span id="lblDocumentTypeRRLL"></span></td>
                                </tr>
                                <tr>
                                    <td class="col-md-6"><label>Nº Documento :</label>&nbsp;&nbsp;<span id="lblNumberDocument"></span></td>
                                    <td class="col-md-6"><label>Nº Documento RRLL :</label>&nbsp;&nbsp;<span id="lblNumberDocumentRRLL"></span></td>
                                </tr>
                                <tr>
                                    <td class="col-md-6"><label>Fecha de Nacimiento :</label>&nbsp;&nbsp;<span id="lblBirthDate"></span></td>
                                    <td class="col-md-6"><label>País de Nacionalidad :</label>&nbsp;&nbsp;<span id="lblCountry"></span></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="panel panel-info">
                        <table class="table table-condensed" >
                            <thead>
                                <tr>
                                    <th><div class="col-md-12"><label>Lista de equipos registrados por el cliente:</label></div></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="col-md-12">
                                        <div class="col-md-12">
                                            <div class="table-responsive">
                                                <table id="tblDetailRecordEquipment" class="table table-bordered claro-modal-tabla" style="width:100%">
                                                    <thead class="claro-bg-primary">
                                                        <tr>
                                                            <th class="col-sm-1 text-center" id="ColumnNro">Línea de Registro</th>
                                                            <th class="col-sm-1 text-center">Fecha de Registro</th>
                                                            <th class="col-sm-1 text-center">IMEI</th>
                                                            <th class="col-sm-1 text-center">Marca - Modelo</th>
                                                            <th class="col-sm-1 text-center">Estado</th>
                                                            <th class="col-sm-1 text-center">Tipo de Bloqueo</th>
                                                            <th class="col-sm-1 text-center">Fecha de Bloqueo</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
      

        </div>
        <div class="col-md-12">
            <div class="col-md-12">
                <form id="formGen" name="formGen">
                    <div class="panel panel-default ">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-2"><label>Solicitante  </label></div>
                                <div class="col-md-10"><p class="text-right label-red-service">(*) Campos Obligatorios</p></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 col-md-12">
                                <div class="col-md-3"><span></span></div>
                                <div class="col-md-9">
                                    <div class="panel panel-default col-md-12" style="padding: 1% !important; margin-bottom: 0px" id="cbotype">
                                        <label id="lblTitular" class="radio-inline col-md-3"><input type="radio" id="cboTitular" name="optradio" checked="checked" value="1">Titular</label>
                                        <label id="lblUsuario" class="radio-inline col-md-3"><input type="radio" id="cboUsuario" name="optradio" value="2">Usuario</label>
                                        <label id="lblRRLLCartaPoder" class="radio-inline col-md-4"><input type="radio" id="cboRRLLCartaPoder" name="optradio" value="4">RRLL/Carta Poder Legalizada</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Nombres : <span class="text-right label-red-service">*</span> </label></div>
                                <div class="col-md-3"><input type="text" class="form-control input-sm" id="txtFirstName" name="txtFirstName" /> </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Apellidos : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3"><input type="text" class="form-control input-sm" id="txtLastName" name="txtLastName" /></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Tipo de Documento : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3"><select class="form-control select-list" id="cbDocumentType" name="cbDocumentType"></select></div>
                                <div class="col-md-2"><label>Nº de Documento : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3"><input type="text" class="form-control input-sm" id="txtDocumentNumber" onkeypress="return f_GetValidTipoDocumento('cbDocumentType', 'txtDocumentNumber', this, event)" /></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Teléfono de Referencia:</label></div>
                                <div class="col-md-3"><input type="text" class="form-control input-sm" id="txtReferencePhone" name="txtReferencePhone" onkeypress="return f_GetValidOnlyNumber('txtReferencePhone', '20', this, event)" /></div>
                                <div class="col-md-2" id="lblRelationshipOwner" style="display:none"><label>Relación con el Titular : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3" id="divtextRelationshipOwner" style="display:none"><select class="form-control select-list" id="cbReasonParient" name="cbReasonParient"></select></div>
                            </div>
                        </div>
                    </div>

                    <div class="panel panel-info ">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12"><label>Datos de la Transacción Asociados al Equipo</label></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Código IMEI : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3"><input class="form-control input-sm" id="txtImei" name="txtImei" onkeypress="return f_GetValidOnlyNumber('txtImei', '15', this, event)" /></div>
                                <div class="col-md-3"><button id="btnSearchIMEI" class="btn btn-primary" type="button">Buscar IMEI</button></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Marca - Modelo : <span class="text-right label-red-service">*</span></label></div>
                                <div class="col-md-3"><input type="text" class="form-control input-sm" id="txtMarkModel" name="txtMarkModel" maxlength="120" /></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-3"><label>Notas :</label></div>
                                <div class="col-md-9"><textarea class="form-control" rows="5" id="txtNotes" name="txtNotes"></textarea></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-3 text-center col-md-offset-5">
                                <div class="panel panel-default" style="padding: 2% !important; margin-bottom: 0px">
                                    <button type="button" class="btn claro-btn-info btn-sm" id="btnSave">&nbsp;&nbsp;Guardar&nbsp;&nbsp;</button>
                                    <button type="button" class="btn claro-btn-info btn-sm" id="btnClose">&nbsp;&nbsp;Cerrar&nbsp;&nbsp;</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
 </div>

<script src="~/Areas/Transactions/Scripts/Redirect/AppRedirect.js"></script>
<script src="~/Areas/Transactions/Scripts/Common/Biometria.js"></script>
<script src="~/Areas/Transactions/Scripts/Common/NoBiometria.js"></script>
<script src="~/Areas/Transactions/Content/Scripts/ClaroAppCommon.js"></script>
@System.Web.Optimization.Scripts.Render("~/bundles/Transactions/RecordEquipmentForeign/RecordEquipmentForeign")