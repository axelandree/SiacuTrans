@{
    ViewBag.Title = "PrepaidBilledOutCallDetails";
     Layout = null;
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="BilledOutCallDetails" class="container-fluid">
    <div class="row">

        <div class="col-md-3">

            <div id="PostpaidDataCustomer" class="panel claro-panel-primary ">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span>
                    <a data-toggle="collapse" role="tab" href="#DatosClientecollapse" class="collapsed transaction-ref-color">
                        Datos del Cliente
                    </a>
                </div>
                <div id="DatosClientecollapse" class="panel-body panel-collapse collapse in">
                    <table class="table table-striped table-responsive table-condensed">
                        <tr>
                            <td><strong>Tipo de Cliente</strong></td>
                            <td><span id="lblCustomerCode"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Contacto</strong></td>
                            <td><span id="lblContactCode"></span></td>
                        </tr>
                        <tr>
                            <td><strong>N° Telefono</strong></td>
                            <td><span id="lblPhone"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Cliente</strong></td>
                            <td><span id="lblCustomerName"></span></td>
                        </tr>

                        <tr>
                            <td><strong>DNI/RUC</strong></td>
                            <td><span id="lblIdentificationDocument"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Plan Actual</strong></td>
                            <td><span id="lblPlan"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Fecha de Activación</strong></td>
                            <td><span id="lblDateActivation"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Representante Legal</strong></td>
                            <td><span id="lblContact"></span></td>
                        </tr>

                    </table>
                </div>

            </div>

        </div>

        <div id="content" class="col-md-9 transaction-content">
            <div class="panel claro-panel-primary">
                <div class="panel-heading ">
                    <a class="collapsed transaction-ref-color">
                        Detalle de Llamadas Salientes Facturado Prepago
                    </a>
                </div>
                <div class="panel-body">
                    <form id="tbDatoTransaccion" class="form-horizontal">
                        <div class="form-group">
                            <label for="txtStartDate" class="col-sm-3 control-label">Desde </label>
                            <div class=" col-sm-2">
                                <div class="input-group">
                                    <input type="text" id="txtStartDate" class="input-sm form-control" name="end" placeholder="DD/MM/AAAA" />
                                    <label class="input-group-addon btn transaction-button" for="txtStartDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>
                            <label for="txtEndDate" class="col-sm-2 control-label">Hasta </label>
                            <div class="col-sm-2">
                                <div class="input-group">
                                    <input type="text" class="input-sm form-control" id="txtEndDate" name="end" placeholder="DD/MM/AAAA">
                                    <label class=" btn btn transaction-button input-group-addon" for="txtEndDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </form>
                    <form class="form-horizontal">
                        <label for="ddlTraficType" class="col-sm-3 control-label">Tipo Tráfico  </label>
                        <div class="col-sm-2">
                            <select class="form-control" id="ddlTraficType">
                                <option value="DSS">Todos</option>
                                <option value="VOZ">VOZ</option>
                                <option value="SMS">SMS</option>
                                <option value="MMS">MMS</option>
                                <option value="DATOS">DATOS</option>
                                <option value="VAS">VAS</option>
                                <option value="USSD">USSD</option>
                                <option value="OTROS">OTROS</option>
                            </select>
                        </div>
                        <br />
                        <br />
                        <br />
                        <div class="col-sm-4">
                            <label class="col-sm-9 control-label">Mostrar todos los registros</label>
                            <div class="col-sm-2 checkbox">
                                <label>
                                    <input type="checkbox" id="chkAllRecords" style="position: relative; top: 3px;" checked="checked">
                                 </label>
                            </div>
                        </div>
                        <div class="col-sm-1">
                            <a id="btnSearch" class="btn transaction-button  btn-sm"><span class="glyphicon glyphicon-search"></span> Buscar</a>
                        </div>
                        <div class="col-sm-2">
                            <a id="btnClear" class="btn transaction-button  btn-sm"><span class="glyphicon glyphicon-erase"></span> Limpiar</a>
                        </div>
                    </form>
                </div>
                <table id="tblBilledOutCalls" class="table table-hover transaction-table table-bordered">
                    <thead>
                        <tr class="btn-danger">
                            <th>N°</th>
                            <th>Fecha Hora</th>
                            <th>Teléfono Destino</th>
                            <th>Tipo de Tráfico</th>
                            <th>Duración</th>
                            <th>Consumo</th>
                            <th>Comprado / Regalado</th>
                            <th>Saldo</th>
                            <th>Bolsa ID</th>
                            <th>Descripcion</th>
                            <th>Plan</th>
                            <th>Promoción</th>
                            <th>Destino</th>
                            <th>Operador</th>
                            <th>Grupo de Cobro</th>
                            <th>Tipo de Red</th>
                            <th>IMEI</th>
                            <th>Roaming</th>
                            <th>Zona Tarifaria</th>
                        </tr>
                    </thead>
                </table>

                <div class="panel-body">
                    <table id="tbDatoCliente" class="table table-condensed">
                        <tr>
                            <td>Total General Voz: </td>
                            <td><label id="lblTotalVoz"></label></td>
                            <td>
                                <label><b>&#124;</b></label>
                            </td>
                            <td>Total General SMS: </td>
                            <td><label id="lblTotalSMS"></label></td>
                            <td>
                                <label><b>&#124;</b></label>
                            </td>
                            <td>Total General MMS: </td>
                            <td><label id="lblTotalMMS"></label></td>
                        </tr>
                        <tr>
                            <td>Total General GPRS(MB): </td>
                            <td><label id="lblTotalGPRS"></label></td>
                            <td>
                                <label><b>&#124;</b></label>
                            </td>
                            <td>Total Registros: </td>
                            <td><label id="lblTotalRegistros"></label></td>
                            <td>
                                <label><b>&#124;</b></label>
                            </td>
                            <td>Total Consumo: </td>
                            <td><label id="lblConsumo"></label></td>
                        </tr>
                    </table>
                    <br />
                </div>
                <div id="divErrorAlert" class="alert alert-transaction alert-danger" role="alert">
                    <button type="button" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <strong><span id="lblErrorMessage"></span></strong>
                </div>
                <form class="form-inline ">
                    <center>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnExport" type="button"><span class="glyphicon glyphicon-export">&nbsp;</span>Exportar</button>
                        </div>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnClose" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                        </div>
                    </center>
                    <br />
                </form>
            </div>
            <div id="myModalLoad" style="display:none;">
                <span>
                    <img src="~/Images/loading2.gif" width="25" height="25" />  Cargando ....
                </span>
            </div>
        </div>

    </div>
</div>

    <input id="hidOpcion" type="hidden" name="hidOpcion" />
    <input id="hidCodOpcion" type="hidden" name="hidCodOpcion" />
    <input id="hidAccion" type="hidden" name="hidAccion" />
    <style>
        .modal-open {
            overflow: scroll;
        }
    </style>

@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
@Scripts.Render("~/bundles/Transactions/Prepaid/BilledOutCallDetail")
