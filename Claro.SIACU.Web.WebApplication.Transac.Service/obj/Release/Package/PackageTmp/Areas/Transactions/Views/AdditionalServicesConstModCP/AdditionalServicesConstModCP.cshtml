
@{
    ViewBag.Title = "AdditionalServicesConstModCP";
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    //Layout = null;
}


<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="AdditionalServicesConstModCP" class="container-fluid">
    
    <div class="tab-content panel-group claro-panel-primary">
        @*<div class="panel-heading">
                Cambio de Cuota de Servicios Postpago
            </div>*@
            
        <div class="panel claro-panel-primary">
            <div class="panel-heading"></div>
            <div class="panel-body">
                <center>
                    <div class="col-sm-4">
                        <p><strong>Nro Teléfono/Servicio</strong></p>
                        <label id="lblNumberPhone"></label>
                    </div>
                    <div class="col-sm-4">
                        <p><strong>Tipo de Cliente</strong></p>
                        <label id="lblTypeClient"></label>
                    </div>
                    <div class="col-sm-4">
                        <p><strong>Contacto</strong></p>
                        <label id="lblContactClient"></label>
                    </div>
                </center>
            </div>
        </div>

            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span>
                    Datos del Cliente
                </div>
                <div class="panel-body">
                    <center>
                        <div class="col-sm-4">
                            <p>
                                <strong>Cliente</strong>
                            </p>
                            <label id="lblClient"></label>
                        </div>
                        <div class="col-sm-4">
                            <p>
                                <strong>DNI/RUC</strong>
                            </p>
                            <label id="lblDNIRUC"></label>
                        </div>
                        <div class="col-sm-4">
                            <p>
                                <strong>Contacto</strong>
                            </p>
                            <label id="lblContact"></label>
                        </div>
                    </center>
                </div>
            </div>
            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-tasks"></span>
                    Datos de la Transacción
                </div>
                <div class="panel-body">
                    @*<div class="row">*@
                    <center>
                        <div class="col-sm-4">
                            <p><strong>Servicio Comercial</strong></p>
                            <label id="lblServiceBusiness"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Cargo Fijo S/. (Sin IGV)</strong></p>
                            <label id="lblChargeFixed"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Cuota Modificada S/. (Sin IGV)</strong></p>
                            <label id="lblQuotModify"></label>
                        </div>
                        @*</div>*@
                        @*<div class="row">*@
                        <div class="col-sm-4">
                            <p><strong>Número de Periodos Válidos</strong></p>
                            <label id="lblNumPerVal"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Nuevo Número de Periodos Válidos</strong></p>
                            <label id="lblNewNumPerVal"></label>
                            <label class="text-danger" id="lblLegend"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Fecha de Ejecución</strong></p>
                            <label id="lblDateExec"></label>
                        </div>
                        @*</div>*@
                        @*<div class="row">*@
                        <div class="col-sm-4">
                            <p><strong>Enviado por Correo</strong></p>
                            <label id="lblSendEmail"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Correo</strong></p>
                            <label id="lblEmail"></label>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Punto de Atención</strong></p>
                            <label id="lblCacDac"></label>
                        </div>
                    @*</div>*@
                    </center>
                </div>
            </div>

            <br />
            <br />

            <div>
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="taskNoteModCP" class="col-sm-2 control-label">Notas</label>
                        <div class="col-sm-8">
                            <textarea class="form-control" rows="4" id="taskNoteModCP"></textarea>
                        </div>
                    </div>
                </form>
            </div>
            <br />

            <div>
                <center>
                    <button id="btnCloseCMCP" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                    <button id="btnConstancyCMCP" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-print"></span> Constancia</button>
                </center>
            </div>
            <br />
        </div>
    

    
   
    <div id="myModalLoad" style="display: none;">
        <span>
            <img src="~/Images/loading2.gif" width="25" height="25"/> Cargando ....
        </span>
    </div>



</div>
@section scripts
{
    <script src="~/Areas/Transactions/Scripts/AdditionalServicesConstModCP/AdditionalServicesConstModCP.js"></script>
    <script src="~/Areas/Transactions/Scripts/AdditionalServices/PostpaidAdditionalServices.js"></script>
}

