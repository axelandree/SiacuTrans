@{
    Layout = null;
}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
        <meta http-equiv="X-UA-Compatible" content="IE=9">
    <title>- SIAC Único</title>
    <link rel="shortcut icon" href="@Url.Content("~/images/favicon.ico")" />  
    @Scripts.Render("~/bundles/jquery-siacu")

    <script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
    <div id="divDeclaracionJuradaNoBiometria" class="container-fluid">
        <input type="hidden" value='@ViewData["motivo"]' id="hidMotivo" />
        <table style="FONT-SIZE: 10pt; FONT-FAMILY: 'Arial','sans-serif'" cellspacing="0" cellpadding="1"
               width="700" align="center" border="0">
            <thead>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <table cellspacing="0" cellpadding="0" width="100%">
                            <tr>
                                <td width="5%"></td>
                                <td width="95%" height="40">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><b>DECLARACIÓN JURADA DE IMPOSIBILIDAD DE VERIFICACIÓN DE IDENTIDAD BIOMETRICA</b></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%" height="30"> <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">I.</p></td>
                                <td width="95%" height="30">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><b>DATOS PERSONALES DEL CLIENTE:</b></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">
                                    <table cellspacing="0" cellpadding="0" width="100%">
                                        <tr>
                                            <td width="25%">
                                                <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify; width: 163px;">Persona Natural</p>
                                            </td>
                                            <td width="25%"><input type="checkbox" id="chkPerNatu" disabled /> </td>
                                            <td class="style1">
                                                <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">Persona Juridica</p>
                                            </td>
                                            <td width="25%"><input type="checkbox" id="chkPerJuri" disabled /> </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">
                                    <table width="100%">
                                        <tr>
                                            <td width="40%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify ; ">Apellidos y Nombres / Razón Social:</p></td>
                                            <td width="60%">
                                                @ViewData["customerFullName"]
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%">&nbsp;</td>
                                <td width="95%">
                                    &nbsp;
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify; height: 27px;">Documento Legal de Identificación:</p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">
                                    <table cellspacing="0" cellpadding="0" width="100%">
                                        <tr>
                                            <td width="10%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">DNI : </p></td>
                                            <td width="10%"><input type="checkbox" id="chkDNI" disabled /></td>
                                            <td width="30%">
                                                @ViewData["dni"]
                                            </td>
                                            <td width="10%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">RUC : </p></td>
                                            <td width="10%"><input type="checkbox" id="chkRUC" disabled /> </td>
                                            <td width="30%"></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>

                            <tr>
                                <td width="5%"> <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">II.</p></td>
                                <td width="95%" height="40">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><b>CAUSAL INDICADA POR LA EMPRESA OPERADORA</b></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">
                                    <table cellspacing="0" cellpadding="0" width="100%">
                                        <tr>
                                            <td width="10%"><input type="checkbox" id="chkCausa1" disabled /></td>
                                            <td width="90%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">Discapacidad física que me impide someterme a la verificación biométrica de huella dactilar.</p></td>
                                        </tr>
                                        <tr>
                                            <td width="10%"><input type="checkbox" id="chkCausa2" disabled /></td>
                                            <td width="90%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">Huella dactilar desgastada que me imposibilita realizar la verificación biométrica.</p></td>
                                        </tr>
                                        <tr>
                                            <td width="10%"><input type="checkbox" id="chkCausa3" disabled /></td>
                                            <td width="90%"><p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">Fallas en la conectividad con la base de datos RENIEC.</p></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"> <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">III.</p></td>
                                <td width="95%" height="40">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><b>INFORMACIÓN OBLIGATORIA</b></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%">

                                    <table cellspacing=0 cellpadding=0 width="100%">
                                        <tbody>
                                            <tr>
                                                <td width="30%">
                                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><span id=lblPregunta1 style="FONT-SIZE: 11pt; TEXT-DECORATION: none; FONT-FAMILY: Arial; WIDTH: 100%; DISPLAY: inline-block">Nombre del Padre</span></p>
                                                </td>
                                                <td width="70%">@ViewData["respuesta1"]</td>
                                            </tr>
                                            <tr>
                                                <td width="30%">
                                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><span id=lblPregunta2 style="FONT-SIZE: 11pt; TEXT-DECORATION: none; FONT-FAMILY: Arial; WIDTH: 100%; DISPLAY: inline-block">Nombre de la Madre</span></p>
                                                </td>
                                                <td width="70%">
                                                    @ViewData["respuesta2"]
                                                </td>
                                            </tr>
                                            <tr>
                                                <td width="30%">
                                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><span id=lblPregunta3 style="FONT-SIZE: 11pt; TEXT-DECORATION: none; FONT-FAMILY: Arial; WIDTH: 100%; DISPLAY: inline-block">Lugar de Nacimiento</span></p>
                                                </td>
                                                <td width="70%">
                                                    @ViewData["respuesta3"]
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>

                                </td>
                            </tr>
                            <tr>
                                <td width="5%"> <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">IV.</p></td>
                                <td width="95%" height="40">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify"><b>SERVICIOS MÓVILES CONTRATADOS</b></p>
                                </td>
                            </tr>
                            <tr id="trLineas" runat="server">
                                <td width="5%"> </td>
                                <td height="95%">

                                    <div>
                                        <table id=dgMoviles style="BORDER-TOP: 2px solid; BORDER-RIGHT: 2px solid; WIDTH: 55%; BORDER-COLLAPSE: collapse; BORDER-BOTTOM: 2px solid; BORDER-LEFT: 2px solid" cellspacing=0 rules=all align=center>
                                            <tbody>
                                                <tr class="FONT-SIZE: 11pt; FONT-FAMILY: Arial;" style="BORDER-TOP-STYLE: solid; FONT-SIZE: smaller; BORDER-LEFT-STYLE: solid; WHITE-SPACE: nowrap; BORDER-BOTTOM-STYLE: solid; BORDER-RIGHT-STYLE: solid" align=center>
                                                    <td>N°</td>
                                                    <td>Número(s) de servicio(s) / IMEI</td>
                                                </tr>
                                                <tr class="FONT-SIZE: 11pt; FONT-FAMILY: Arial;" style="BORDER-TOP-STYLE: solid; FONT-SIZE: smaller; BORDER-LEFT-STYLE: solid; BORDER-BOTTOM-STYLE: solid; BORDER-RIGHT-STYLE: solid; BACKGROUND-COLOR: white" align=center>
                                                    <td style="WIDTH: 70px; FONT-WEIGHT: normal; BACKGROUND-COLOR: white" align=center>1</td>
                                                    <td style="WIDTH: 180px; FONT-WEIGHT: normal; BACKGROUND-COLOR: white" align=center>@ViewData["customerTelephone"]</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%" class="style2"> <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify">V.</p></td>
                                <td width="95%" class="style2">
                                    <p style="FONT-SIZE: 11pt; FONT-FAMILY: Arial; TEXT-ALIGN: justify; font-weight: bold; position: static;"
                                       align="left">DECLARO BAJO JURAMENTO QUE, EN LA CONTRATACIÓN DE LOS SERVICIOS ANTES MENCIONADOS NO HA PODIDO REALIZARSE LA VERIFICACIÓN BIOMÉTRICA.</p>
                                </td>
                            </tr>
                            <tr>
                                <td width="5%"></td>
                                <td width="95%" height="40">
                                    <br><br><br>
                                    <table width="100%">
                                        <tr>
                                            <td width="5%"></td>
                                            <td width="95%" align="left">
                                                <p style="TEXT-ALIGN: justify">@ViewData["lblFecha"]</p>
                                            </td>
                                        </tr>
                                    </table>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td height="150">
                        <table cellspacing="0" cellpadding="0" width="50%" align="center">
                            <tr>
                                <td style="BORDER-TOP: black 1px solid; FONT-SIZE: 8pt; FONT-FAMILY: Verdana; HEIGHT: 18px"
                                    align="center" width="45%">
                                    <b>FIRMA</b>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </tbody>
        </table>
        <div class="row col-md-6" style="text-align:center;">
            <button id="btnImprimir" type="button" class="btn transaction-button btn-sm">Imprimir</button>
            <button id="btnCerrar" type="button" class="btn transaction-button btn-sm">Cerrar</button>

        </div>
    </div>

    <script src="~/Areas/Transactions/Scripts/Common/DeclaracionJuradaNoBiometria.js"></script>
