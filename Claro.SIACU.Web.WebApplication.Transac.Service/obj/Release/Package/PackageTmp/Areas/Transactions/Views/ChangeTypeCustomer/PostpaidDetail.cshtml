                            <tr>
                                <td><strong>Cliente</strong></td>
                                <td><label id="lblPost_BusinessName"></label></td>
                                <td><strong>DNI/RUC</strong></td>
                                <td><label id="lblPost_DocumentNumber"></label></td>
                            </tr>
                            <tr>
