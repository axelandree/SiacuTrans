
@{
    
    Layout = null;
    
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<input id="hidOpcion" type="hidden" name="hidOpcion" />
<div id="PostpaidIncomingCallDetail" class="container-fluid">
    <br />
        <div class="col-md-3">
            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span> 
                    <a data-toggle="collapse" href="#Datoscliente" class="collapsed transaction-ref-color">
                       Datos del Cliente
                    </a>
                </div>
                <div id="Datoscliente" class="panel-body panel-collapse collapse in">
                    <table class="table table-striped table-condensed">
                        <tr>
                            <td><strong>Cliente </strong></td>
                            <td><label id="lblCustomerName" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>Contacto del Cliente </strong></td>
                            <td><label id="lblContactCustomer" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>Contacto </strong></td>
                            <td><label id="lblContact" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>Representante Legal </strong></td>
                            <td><label id="lblLegalRepresentative" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>DNI/RUC </strong></td>
                            <td><label id="lblIdentificationDocument" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>Doc. Repres. Legal </strong></td>
                            <td><label id="lblIdentificationDocumentoLR" class="text-message-info"></label></td>
                        </tr>
                        <tr>
                            <td><strong>Fecha de Activación </strong></td>
                            <td><label id="lblActivationDate" class="text-message-info"></label></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div id="content" class="col-md-9 transaction-content">
                <div class="panel claro-panel-primary">
                    <div class="panel-heading">
                        <span class="glyphicon"></span>Detalle De Llamadas Entrantes Postpago
                    </div>
                    <div class="panel-body">
                        <form class="form-horizontal">
                            <div class="form-group">

                                <label class="col-sm-2 control-label">Desde</label>
                                <div class="col-sm-3">
                                    <div class="input-group input-group-sm">
                                        <input type="text" class="form-control" id="txtStartDate" placeholder="DD/MM/AAAA">
                                        <label id="calendartxtStartDate" class="btn transaction-button input-group-addon" for="txtStartDate">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </label>
                                    </div>
                                </div>
                                
                                <label class="col-sm-2 control-label">Hasta</label>
                                <div class="col-sm-3">
                                    <div class="input-group input-group-sm">
                                        <input type="text" class="form-control input-sm" id="txtEndDate" placeholder="DD/MM/AAAA">
                                        <label id="calendartxtEndDate" class="btn transaction-button input-group-addon " for="txtEndDate">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </label>

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="ddlCACDAC" class="col-sm-2 control-label">Punto de Atención </label>
                                <div class="col-sm-3">
                                    <select class="form-control input-sm" id="ddlCACDAC"></select>
                                </div>
                               

                                <label class="col-sm-2 control-label">Generar OCC</label>
                                <div class="col-sm-1 checkbox">  
                                    <label>
                                        <input type="checkbox" id="chkGeneraOCC" value="true" />
                                    </label>
                                </div>
                               
                                <div class="col-sm-2">
                                    <button id="btnConsult" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-search"></span> Consultar</button>
                                </div>
                            </div>
                        </form>
                        <div>
                            <table id="tblIncomingCalls" class="table table-bordered table-hover table-striped table-responsive">
                            
                                <thead class="btn-danger">
     
                                        <tr>
                                        <th>N°</th>
                                        <th>Número</th>
                                        <th>Fecha</th>
                                        <th>Hora</th>
                                        <th>Número Entrante</th>
                                        <th>Duración</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <br /> 
                        <div>
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label for="tareaNotas" class="col-sm-2 control-label">Notas</label>
                                    <div class="col-sm-8">
                                        <textarea class="form-control" rows="4" id="taskNote"></textarea>
                                    </div>
                                </div>
                            </form>
                           
                            <div id="divShowMessage" class="alert alert-transaction alert-danger" role="alert">
                                <button id="btnCloseError" type="button" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                <strong><span id="lblErrorMessage"></span></strong>
                            </div>
                        </div>
                        <div>
                            <center>
                                <button id="btnSave" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                                <button type="button" class="btn transaction-button btn-sm" data-toggle="modal" id="btnPrint"><span class="glyphicon glyphicon-print"></span> Imprimir</button>
                                <button id="btnExport" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-save"></span>Exportar</button>
                                <button id="btnConstancy" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-list-alt"></span> Constancia</button>
                                <button id="btnClose" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                            </center>
                        </div>
                        <br />
                    </div>
                </div>
        </div>  
</div>
@Scripts.Render("~/bundles/Transactions/ReadRecordPDF/ReadRecord");
@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
@Scripts.Render("~/bundles/Transactions/Postpaid/IncomingCallDetail")