@{
    ViewBag.Title = "Cambio de Número";
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    ViewBag.TitleHtml = "CAMBIO DE NÚMERO";
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="PostPaidChangePhoneNumber" class="contenedor">
    <div class="scrollbudy">
        <div class="col-md-3">

            <div class="panel-tabs">

                <ul class="nav nav-tabs" role="tablist" data-tabs="tabs">
                    <li role="presentation" class="active"><a href="#customer" aria-controls="customer" role="tab" data-toggle="tab"><strong><span class="glyphicon glyphicon-user"></span> Datos Cliente</strong></a></li>
                    <li role="presentation"><a href="#business" aria-controls="business" role="tab" data-toggle="tab"><strong><span class="glyphicon glyphicon-th-list"></span> Reglas de Atención</strong></a></li>
                </ul>

                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="customer">
                        <table class="table table-condensed">
                            <tbody>
                                <tr>
                                    <td class="col-md-3">Nro. Teléfono</td>
                                    <td class="col-md-3"><label id="lblPhoneNumber"></label></td>
                                </tr>
                                <tr>
                                    <td class="col-md-3">Tipo de Cliente</td>
                                    <td class="col-md-3"><label id="lblCustomerType"></label></td>
                                </tr>
                                <tr>
                                    <td class="col-md-3">Cliente</td>
                                    <td class="col-md-3"><label id="lblCustomerName"></label></td>
                                </tr>
                                <tr>
                                    <td class="col-md-3">Contacto del cliente</td>
                                    <td class="col-md-3"><label id="lblCustomerContact"></label></td>
                                </tr>
                                <tr>
                                    <td>Contacto</td>
                                    <td><label id="lblContactCust"></label></td>
                                </tr>
                                <tr>
                                    <td>Representante Legal</td>
                                    <td><label id="lblLegalRepresentative"></label></td>
                                </tr>
                                <tr>
                                    <td>DNI/RUC</td>
                                    <td><label id="lblIdentificationDocument"></label></td>
                                </tr>
                                <tr>
                                    <td>Doc. Repres. Legal</td>
                                    <td><label id="lblIdentificationDocumentoLR"></label></td>
                                </tr>
                                <tr>
                                    <td>Fecha de Activación</td>
                                    <td><label id="lblActivationDate"></label></td>
                                </tr>
                                <tr>
                                    <td>Fecha ult. Recarga</td>
                                    <td><label id="lblLastDateRecharge"></label></td>
                                </tr>
                                <tr>
                                    <td>Fecha de Nacimiento</td>
                                    <td><label id="lblBornDate"></label></td>
                                </tr>
                                <tr>
                                    <td>Monto ult. Recarga</td>
                                    <td><label id="lblLastMountRecharge"></label></td>
                                </tr>
                                <tr>
                                    <td>Número triados</td>
                                    <td><label id="lblNumberTriado"></label></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div role="tabpanel" class="tab-pane fade" id="business">
                        <div id="divRules">

                        </div>
                    </div>
                </div>

            </div>



        </div>
        <div class="col-md-9">

            <!-- General Container - Header Step -->
            <div class="transaction-container-step">
                <!-- Container - Circle Button -->
                <div class="process-2-step">
                    <!-- Container - Bootstrap Tab -->
                    <div class="process-row nav nav-tabs">
                        <!-- Container - Step 1 -->
                        <div class="process-step process-step-6-left">
                            <button type="button" class="btn btn-circle transaction-button" data-toggle="tab" role="tab" href="#tabChangePhoneNumber" percent="0%"><i class="fa fa-exchange" aria-hidden="true"></i></button>
                            <p> Cambio</p>
                        </div>
                        <!-- Container - Step 2 -->
                        <div class="process-step process-step-6-right">
                            <button type="button" class="btn btn-circle disabled" data-toggle="tab" role="tab" href="#tabSummary" percent="99%" disabled><i class="fa fa-list-ul" aria-hidden="true"></i></button>
                            <p>Resumen</p>
                        </div>
                    </div>
                </div>
                <!-- Contenedor del progress bar -->
                <div class="transaction-progress-2-step">
                    <div class="progress transaction-bar">
                        <div id="prog" class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                        </div>
                    </div>
                </div>

            </div>

            <!-- General Container - Body Step -->
            <div class="tab-content">
                <div id="tabChangePhoneNumber" class="step tab-pane active" role="tabpanel" percent="0%">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label for="ddlLocation" class="col-sm-3 control-label">Localidad Nuevo Número</label>
                                    <div class="col-sm-3">
                                        <select id="ddlLocation" class="form-control input-sm" data-bind-change="Region"></select>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="checkbox">
                                            <label>
                                                <input id="chkTranslationNumber" type="checkbox" data-bind-change="Check" disabled>
                                                Traslado Número
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtStartHLR" class="col-sm-3 control-label">HLR Inicial</label>
                                    <div class="col-sm-2">
                                        <input type="text" id="txtStartHLR" class="form-control input-sm" data-bind-change="StartHLR" readonly />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtNewNumber" class="col-sm-3 control-label">Nuevo número</label>
                                    <div class="col-sm-3">
                                        <input type="text" id="txtNewNumber" class="form-control input-sm" data-bind-change="NewPhoneNumber" readonly />
                                    </div>
                                    <div class="col-sm-3">
                                        <button id="btnSearch" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-search"></span> Buscar</button>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtEndHLR" class="col-sm-3 control-label">HLR Final</label>
                                    <div class="col-sm-2">
                                        <input type="text" id="txtEndHLR" class="form-control input-sm" data-bind-change="EndHLR" readonly />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Fidelizar</label>
                                    <div class="col-sm-3">
                                        <label class="radio-inline">
                                            <input type="radio" name="Fidelize" id="rdFidelizeYes" value="1"> SI
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="Fidelize" id="rdFidelizeNot" value="2"> NO
                                        </label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtCostTrans" class="col-sm-3 control-label">Costo de la transacción</label>
                                    <div id="divCostoTrans" class="col-sm-2">
                                        <input type="text" id="txtCostTrans" class="form-control input-sm text-right" />
                                    </div>
                                    <div id="divCostTransCorp" class="col-sm-2">
                                        <select id="ddlCostCorp" class="form-control input-sm text-right"></select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlCACDAC" class="col-sm-3 control-label">CAC / DAC</label>
                                    <div class="col-sm-3">
                                        <select id="ddlCACDAC" class="form-control input-sm"></select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtNotas" class="col-sm-3 control-label">Notas</label>
                                    <div class="col-sm-6">
                                        <textarea id="taNotas" class="form-control" rows="3" data-bind-change="Notes"></textarea>
                                    </div>
                                </div>
                                <div class="alert alert-danger alert-dismissible" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                    <label id="lblErrorMessage"></label>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12 text-right">
                                        <div class="btn-group" role="group" aria-label="...">
                                            <button type="button" class="btn transaction-button prev-step" disabled><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Ant. </button>
                                            <button id="btnStep1Next" type="button" class="btn transaction-button next-trans">Sig. <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div id="tabSummary" class="step tab-pane fade" role="tabpanel" percent="99%">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <table class="table table-condensed">
                                <tbody>
                                    <tr>
                                        <td>Localidad</td>
                                        <td><label id="lblRegion" data-bind-change="RegionDesc"></label></td>
                                        <td>Cambio/Traslado</td>
                                        <td><label id="lblChkTraslate" data-bind-change="Translate"></label></td>
                                    </tr>
                                    <tr>
                                        <td>HLR Inicial</td>
                                        <td><label id="lblStartHLR" data-bind-change="StartHLR"></label></td>
                                        <td>CAC/DAC</td>
                                        <td><label id="lblCACDAC" data-bind-change="CACDACDesc"></label></td>
                                    </tr>
                                    <tr>
                                        <td>HLR Final</td>
                                        <td><label id="lblEndHLR" data-bind-change="EndHLR"></label></td>
                                        <td>Nuevo número</td>
                                        <td><label id="lblNewPhoneNumber" data-bind-change="NewPhoneNumber"></label></td>
                                    </tr>
                                    <tr>
                                        <td>Fidelizar</td>
                                        <td>SI</td>
                                        <td>Costo</td>
                                        <td>0.00</td>
                                    </tr>
                                    <tr>
                                        <td>Notas</td>
                                        <td><label id="lblNotes" data-bind-change="Notes"></label></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row">
                                <div class="col-sm-9 text-center">
                                    <div class="btn-group" role="group" aria-label="...">
                                        <button id="btnSave" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                                        <button id="btnConstancy" type="button" class="btn transaction-button" disabled><span class="glyphicon glyphicon-list-alt"></span> Constancia</button>
                                    </div>
                                </div>
                                <div class="col-sm-3 text-right">
                                    <div class="btn-group" role="group" aria-label="...">
                                        <button type="button" class="btn transaction-button prev-step"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Ant. </button>
                                        <button type="button" class="btn transaction-button next-step" disabled>Sig. <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div class="clearfix"></div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript" src="~/Areas/Transactions/Scripts/ChangePhoneNumber/PostpaidChangePhoneNumber.js"></script>
}