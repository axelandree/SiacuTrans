@{
    //Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    Layout = null;
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<input id="hidOpcion" type="hidden" name="hidOpcion" />
<div id="PrepaidIncomingCallDetail" class="container-fluid">

   
    <div class="col-md-3">
        <div class="panel claro-panel-primary">
            <div class="panel-heading">
                <span class="glyphicon glyphicon-user"></span>
                <a data-toggle="collapse" href="#DatosCliente" class="collapsed transaction-ref-color">
                    Datos del Cliente
                </a>
            </div>
            <div id="DatosCliente" class="panel-body panel-collapse collapse in">
                <table class="table table-striped table-responsive table-condensed">
                    <tr>
                        <td><strong>Cliente </strong></td>
                        <td><label id="lblCustomerName" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Cliente Codigo </strong></td>
                        <td><label id="lblContactCustomer" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Contacto Codigo </strong></td>
                        <td><label id="lblContact" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Direccion</strong></td>
                        <td><label id="lblAddressCustomer" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Email</strong></td>
                        <td><label id="lblEmailCustomer" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>DNI/RUC </strong></td>
                        <td><label id="lblIdentificationDocument" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Doc. Repres. Legal </strong></td>
                        <td><label id="lblIdentificationDocumentoLR" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Fecha de Activación </strong></td>
                        <td><label id="lblActivationDate" class="text-message-info"></label></td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
    <div id="content" class="col-md-9 transaction-content">
        <div class="panel claro-panel-primary">
            <div class="panel-heading">
                Detalle de Llamadas Entrante Prepago
            </div>
            <div class="panel-body">
                <form class="form-horizontal">
                    <div class="form-group">

                        <label for="txtStartDate" class="col-sm-2 control-label">Desde</label>
                        <div class="col-sm-3">
                            <div class="input-group input-group-sm">
                                <input type="text" class="form-control input-sm" id="txtStartDate" placeholder="">
                                <label class="btn transaction-button input-group-addon" for="txtStartDate">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </label>
                            </div>
                        </div>


                        <label for="txtEndDate" class="col-sm-2 control-label">Hasta</label>
                        <div class="col-sm-3">
                            <div class="input-group input-group-sm">
                                <input type="text" class="form-control input-sm" id="txtEndDate">
                                <label class="btn transaction-button input-group-addon" for="txtEndDate">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </label>
                            </div>
                        </div>

                    </div>
                    <div class="form-group">
                        <label for="ddlCACDAC" class="col-sm-2">Punto de Atención</label>
                        <div class="col-sm-3">
                            <div class="input-group">
                                <select class="form-control input-sm-2" id="ddlCACDAC"></select>
                            </div>
                        </div>
                        <label for="chkGenerateOCC" class="col-sm-2  control-label">Generar OCC</label>
                        <div class="col-sm-1 checkbox">
                                <label>
                                    <input  type="checkbox" id="chkGenerateOCC" checked="checked">
                                </label>
      
                        </div>
                        <div class="col-sm-2">
                            <input type="text" class="form-control input-sm-2" id="txtOCC">
                        </div>
                        <div class="col-sm-2">
                            <button type="button" id="btnConsult" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-search"></span> Consultar</button>
                        </div>
                    </div>
                </form>
            </div>


            <div>
                <table id="tblIncomingCalls" class="table table-bordered table-hover table-striped">
                    <thead class="btn-danger">
                        <tr>
                            <th>N°</th>
                            <th>Número</th>
                            <th>Fecha</th>
                            <th>Hora</th>
                            <th>Número Entrante</th>
                            <th>Duración</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>


            <div>
                <form class="form-horizontal">
                    <div class="form-group">
                        <label id="lblSummary" class="col-sm-offset-1 col-sm-11 text-info">Total Registros: 0</label>
                    </div>
                    <div class="form-group">
                        <div id="divErrorAlert" class="alert alert-transaction alert-danger" role="alert">
                            <strong><span id="lblErrorMessage" class="col-sm-offset-1">Mensaje</span></strong>
                        </div>
                    </div>
                </form>
            </div>

            <div>
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="txtaNotas" class="col-sm-2 control-label">Notas</label>
                        <div class="col-sm-8">
                            <textarea class="form-control input-sm" rows="4" id="txtaNotas"></textarea>
                        </div>
                    </div>
                </form>
            </div>

            <center>
                <button id="btnSave" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                <button id="btnPrint" type="button" class="btn transaction-button btn-sm" data-toggle="modal"><span class="glyphicon glyphicon-print"></span> Imprimir</button>
                <button id="btnExport" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-save"></span> Exportar</button>
                <button id="btnConstancy" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-list-alt"></span> Constancia</button>
                <button id="btnClose" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
            </center>
            <br />
        </div>
    </div>
  
</div>

@Scripts.Render("~/bundles/Transactions/IncomingCallDetail/PrepaidIncomingCallDetail");
@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
@Scripts.Render("~/bundles/Transactions/ReadRecordPDF/ReadRecord")
