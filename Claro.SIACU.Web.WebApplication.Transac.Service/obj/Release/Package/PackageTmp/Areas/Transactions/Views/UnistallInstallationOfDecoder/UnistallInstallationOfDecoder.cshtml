@{
    //ViewBag.Title = "UnistallInstallationOfDecoder";
    //Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    //ViewBag.TitleHtml = "Instalación/Desinstalación de Decodificador";
    Layout = null;
}

<style>
    table > tbody > tr > td.border-top {
        border-top: 0px;
    }

    textarea.form-control {
        height: 100px;
    }

    .modalInsDes {
        position: relative;
        border: none;
        bottom: 250px;
        background-color: #000;
        -webkit-border-radius: 10px;
        -moz-border-radius: 10px;
        opacity: .5;
        color: #fff;
        margin-left: 270px;
        margin-right: 270px;
        padding: 10px;
    }

    div#HFCListAddtionalEquipment > div > div {
        margin-bottom: 0 !important;
    }

    body {
        padding-right: 0 !important;
    }
</style>
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="UnistallInstallationOfDecoder" class="container-fluid">
    <div class="scrollbudy">
        <div class="col-md-3">
            <div id="DataCustomer" class="panel claro-panel-primary panel-group" role="tablist" aria-multiselectable="true">
                <div id="customerDataHead" role="tab" class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span>
                    <a data-toggle="collapse" href="#customerDataCollapse" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Datos del Cliente</a>
                </div>
                <div class="panel-body panel-collapse collapse in" id="customerDataCollapse" role="tabpanel" aria-labelledby="customerDataHead">
                    <table class="table table-condensed">
                        <tr>
                            <td class="col-md-3">Contrato:</td>
                            <td class="col-md-3"><label id="lblIDContrato"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Customer ID:</td>
                            <td class="col-md-3"><label id="lblIDCustomer"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Tipo Cliente:</td>
                            <td class="col-md-3"><label id="lblTipoCliente"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Cliente:</td>
                            <td class="col-md-3"><label id="lblCliente"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Contacto:</td>
                            <td class="col-md-3"><label id="lblContacto"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">DNI / RUC:</td>
                            <td class="col-md-3"><label id="lblDNIRUC"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Representante Legal:</td>
                            <td class="col-md-3"><label id="lblRepresentanteLegal"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Plan Actual:</td>
                            <td class="col-md-3"><label id="lblPlanActual"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Fecha de Activación:</td>
                            <td class="col-md-3"><label id="lblFechaActivacion"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Ciclo de Facturación:</td>
                            <td class="col-md-3"><label id="lblCicloFacturacion"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Límite de Crédito:</td>
                            <td class="col-md-3"><label id="lblLimiteCredito"></label></td>
                        </tr>
                    </table>
                </div>

                <div id="addressDataHead" role="tab" class="panel-heading">
                    <span class="glyphicon glyphicon-home"></span>
                    <a data-toggle="collapse" href="#addressDataCollapse" aria-expanded="false" data-parent="#DataCustomer" aria-controls="addressDataCollapse" class="collapsed transaction-ref-color"> Dirección de Facturación</a>
                </div>
                <div class="panel-body panel-collapse collapse" id="addressDataCollapse" role="tabpanel" aria-labelledby="addressDataHead">
                    <table class="table table-condensed">
                        <tr>
                            <td class="col-md-3">Dirección:</td>
                            <td class="col-md-3"><label id="lblDireccion"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Referencia:</td>
                            <td class="col-md-3"><label id="lblReferencia"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Pais:</td>
                            <td class="col-md-3"><label id="lblPais"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Departamento:</td>
                            <td class="col-md-3"><label id="lblDepartamento"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Provincia:</td>
                            <td class="col-md-3"><label id="lblProvincia"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Distrito:</td>
                            <td class="col-md-3"><label id="lblDistrito"></label></td>
                        </tr>
                        <tr>
                            <td class="col-md-3">Código de Plano:</td>
                            <td class="col-md-3"><label id="lblCodigoPlano"></label></td>
                        </tr>
                    </table>
                </div>
            </div>

        </div>
        <div id="content" class="col-md-9 transaction-content">
            <div class="transaction-container-step ">
                <div class="process">
                    <div class="process-row nav nav-tabs">
                        <div class="process-step process-step-3-left">
                            <button type="button" class="btn btn-circle transaction-button" data-toggle="tab" role="tab" href="#tabProducts" percent="0%" index="1"><i class="fa fa-table" aria-hidden="true"></i></button>
                            <p>Lista de Equipos</p>
                        </div>
                        <div class="process-step process-step-3">
                            <button type="button" class="btn btn-circle disabled" data-toggle="tab" role="tab" href="#tabTypeWorkFid" percent="37.5%" index="2" disabled><i class="fa fa-legal" aria-hidden="true"></i></button>
                            <p>Tipo Trabajo</p>
                        </div>
                        <div class="process-step process-step-3">
                            <button type="button" class="btn btn-circle disabled" data-toggle="tab" role="tab" href="#tabTrasaction" percent="62.5%" index="3" disabled><i class="fa fa-envelope" aria-hidden="true"></i></button>
                            <p>Datos Técnicos</p>
                        </div>
                        <div class="process-step process-step-3-right">
                            <button type="button" class="btn btn-circle disabled" data-toggle="tab" role="tab" href="#tabSummary" percent="99%" index="4" disabled id="btnSummary02"><i class="fa fa-list-ul" aria-hidden="true"></i></button>
                            <p>Resumen</p>
                        </div>
                    </div>
                </div>
                <div class="transaction-progress">
                    <div class="progress transaction-bar">
                        <div id="prog" class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-content">
                <div id="tabProducts" class="step tab-pane active" role="tabpanel" percent="0%">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <form class="form-horizontal">
                                <!--Equipos Asociados al Cliente-->
                                <table id="tblDetalleProducto" class="table table-bordered table-striped">
                                    <thead class="btn-danger">
                                        <tr>
                                            <th>Cod. Material</th>
                                            <th>Cod. SAP</th>
                                            <th>Num. Serie</th>
                                            <th>Mac Ad.</th>
                                            <th>Desc. Material</th>
                                            <th>Tipo Equipo</th>
                                            <th>Código / Id Producto</th>
                                            <th>Modelo</th>
                                            <th>Estado</th>
                                            @*<th>Conv. Type</th>*@
                                            <th>Serv/Paque. Principal</th>
                                            @*<th>Head End</th>
                                            <th>Perfil</th>
                                            <th>Número</th>*@
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </form>
                            <div class="form-group">
                                <center>
                                    <button type="button" class="btn transaction-button next-step" id="btnSiguiente01">Siguiente <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
                                    <button id="btnCerrar01" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                                </center>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tabTypeWorkFid" class="step tab-pane fade" role="tabpanel" percent="37.5%">
                    <form class="form-horizontal">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Opción:</label>
                                    <div class="col-sm-4">
                                        <div class="btn-group" data-toggle="buttons">
                                            <label class="btn transaction-radio-button active" id="lblInstalacion">
                                                <input type="radio" name="options" id="rbtInstalacion" autocomplete="off" checked> Instalación
                                            </label>
                                            <label class="btn transaction-radio-button" id="lblDesinstalacion">
                                                <input type="radio" name="options" id="rbtDesinstalacion" autocomplete="off"> Desinstalación
                                            </label>
                                        </div>
                                        @*<label class="radio-inline">
                                            <input type="radio" name="inlineRadioOptions" id="rbtInstalacion" value="option1" checked>
                                            <strong> Instalación</strong>
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="inlineRadioOptions" id="rbtDesinstalacion" value="option2">
                                            <strong> Desinstalación</strong>
                                        </label>*@
                                    </div>
                                    <label class="col-sm-3 control-label">Consultar Equipos:</label>
                                    <div class="col-sm-3">
                                        <button id="btnConsultarEquipos" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-option-horizontal"></span></button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Tipo de Trabajo:</label>
                                    <div class="col-sm-4">
                                        <select id="cboTipoTrabajo" class="form-control input-sm " disabled></select>
                                    </div>
                                    <label class="col-sm-3 control-label">Sub Tipo de Trabajo:</label>
                                    <div class="col-sm-3">
                                        <select id="cboSubTipoTrabajo" class="form-control input-sm " disabled></select>
                                    </div>
                                </div>

                                @Html.Partial("~/Areas/Transactions/Views/Shared/ViewPartial/Schedule.cshtml",
                                                    new ViewDataDictionary {
                                                        { "Text_Id", "txtFProgramacion" },
                                                        { "Select_Id", "cboFranjaHoraria" },
                                                        { "Button_Id", "btnValidateSchedule" },
                                                        { "Combo_JobTypes" , "cboTipoTrabajo"},
                                                        { "Combo_SubJobType" , "cboSubTipoTrabajo"},
                                                        { "Input_Size" , "input-xs"},
                                                        { "strDateServer", @ViewData["strDateServer"] },
                                                        { "strDateNew", @ViewData["strDateNew"] },
                                                        { "Button_Save", "btn_saveTransaction"}
                                            })

                            </div>
                        </div>

                        <div class="panel panel-default">
                            <div class="panel-body">
                                <!--Equipos Adicionales-->
                                <table id="tblEquipos" class="table table-bordered table-striped">
                                    <thead class="btn-danger">
                                        <tr>
                                            <th></th>
                                            <th>Nombre del Equipo @*Nombre del Servicio*@</th>
                                            <th>Tipo de Equipo @*Tipo de Servicio*@</th>
                                            <th>Grupo de Servicio</th>
                                            <th>Cargo Fijo (S/) Sin IGV</th>
                                            <th>Cargo Fijo (S/) Con IGV</th>
                                            <th>Equipo</th>
                                            <th>Cant.</th>
                                            <th>SN CODE.</th>
                                            <th>SP CODE.</th>
                                            <th>Tipo Eq.</th>
                                            <th>TM Code.</th>
                                            <th>Cod. Serv.</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>

                                <div class="form-group">
                                    <div class="col-sm-5">
                                        <table class="table table-condensed">
                                            <tr>
                                                <td class="border-top"><strong>Cantidad de Equipo en Alquiler Agregados:</strong></td>
                                                <td class="border-top text-right"><label id="lblCantidad"></label></td>
                                            </tr>
                                            <tr>
                                                <td class="border-top"><strong>Cargo Fijo Total (SIN IGV):</strong></td>
                                                <td class="border-top text-right">S/ <label id="lblCargoFijoTotalPlanSIGV"></label></td>
                                            </tr>
                                            <tr>
                                                <td class="border-top"><strong>Cargo Fijo Total (CON IGV):</strong></td>
                                                <td class="border-top text-right">S/ <label id="lblCargoFijoTotalPlanCIGV"></label></td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-condensed">
                                    <tr>
                                        <td class="border-top" style="width: 10%"><strong>Fidelizar</strong></td>
                                        <td class="border-top" style="width: 10%"><input type="checkbox" id="chkFidelizacion" value=""></td>
                                        <td class="border-top" style="width: 30%"><strong id="lblCosto">Costo de Instalación (Con IGV):</strong></td>
                                        <td class="border-top" style="width: 50%">S/ <label id="lblMontoFidelizacion"></label></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </form>
                    <div class="form-group">
                        <center>
                            <button type="button" class="btn transaction-button prev-step"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Anterior</button>
                            <button type="button" class="btn transaction-button next-step btn_saveTransaction" id="btnSiguiente02">Siguiente <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
                            <button id="btnCerrar02" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                        </center>
                    </div>
                </div>
                <div id="tabTrasaction" class="step tab-pane fade" role="tabpanel" percent="62.5%">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Nota(*):</label>
                                    <div class="col-sm-8">
                                        <textarea id="txtNota" class="form-control input-sm"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Enviar por Email:</label>
                                    <div class="col-sm-1">
                                        <div class="checkbox">
                                            <label>
                                                <input id="chkEnviarPorEmail" type="checkbox" value="">
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-sm-7">
                                        <input type="text" class="form-control input-sm" id="txtEnviarporEmail" style="display: none" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Punto de Atención:</label>
                                    <div class="col-sm-3">
                                        <select id="ddlCACDAC" class="form-control input-sm"></select>
                                    </div>
                                </div>
                            </form>
                            <div class="form-group">
                                <center>
                                    <button type="button" class="btn transaction-button prev-step"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Anterior</button>
                                    <button type="button" class="btn transaction-button next-step" id="btnSiguiente03">Siguiente <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
                                    <button id="btnCerrar03" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                                </center>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tabSummary" class="step tab-pane fade" role="tabpanel" percent="99%">
                    <div class="panel claro-panel-primary">
                        <div id="tabSummary" role="tab" class="panel-heading">
                            <span class="glyphicon glyphicon-user"></span>
                            <label data-toggle="collapse" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Resumen</label>
                        </div>
                        <div class="panel-body">
                            <table class="table table-condensed table-responsive">
                                <tbody>
                                    <tr class="active">
                                        <td colspan="4"><label class="control-label"><i class="fa fa-legal" aria-hidden="true"></i> Tipo Trabajo</label></td>
                                    </tr>
                                    <tr>
                                        <td width="25%">Opción</td>
                                        <td><label data-bind-transfer="Opcion"></label></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>Tipo de Trabajo</td>
                                        <td><label data-bind-transfer="TipoTrabajo"></label></td>
                                        <td>Sub Tipo de Trabajo</td>
                                        <td><label data-bind-transfer="SubTipoTrabajo"></label></td>
                                    </tr>
                                    <tr>
                                        <td>Fecha Compromiso</td>
                                        <td><label data-bind-transfer="FechaCompromiso"></label></td>
                                        <td>Horario</td>
                                        <td><label data-bind-transfer="Horario"></label></td>
                                    </tr>
                                    <tr>
                                        <td colspan="4">
                                            <table class="table table-bordered" style="margin-top:10px;margin-bottom:10px;">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center">Nombre del Equipo</th>
                                                        <th class="text-center">Tipo del Equipo</th>
                                                        <th class="text-center">Cargo Fijo (S/) Sin IGV</th>
                                                        <th class="text-center">Cargo Fijo (S/) Con IGV</th>
                                                    </tr>
                                                </thead>
                                                <tbody data-bind-transfer="ListEqInstBaja"></tbody>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Cargo Fijo Total (SIN IGV)</td>
                                        <td><label data-bind-transfer="CargoFijoSIGV"></label></td>
                                        <td>Cargo Fijo Total (CON IGV):</td>
                                        <td><label data-bind-transfer="CargoFijoCIGV"></label></td>
                                    </tr>
                                    <tr>
                                        <td>Fidelizar</td>
                                        <td><label data-bind-transfer="Fidelizar"></label></td>
                                        <td id="lblSumaryTipoCosto">Costo de Instalación (Con IGV)</td>
                                        <td><label data-bind-transfer="MontoFidelizar"></label></td>
                                    </tr>
                                    <tr class="active">
                                        <td colspan="4"><label class="control-label"><i class="fa fa-envelope" aria-hidden="true"></i> Datos Técnicos</label></td>
                                    </tr>
                                    <tr>
                                        <td>Nota</td>
                                        <td colspan="3"><label data-bind-transfer="Nota"></label></td>
                                    </tr>
                                    <tr>
                                        <td>Correo</td>
                                        <td><label data-bind-transfer="Correo"></label></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>Punto de Venta</td>
                                        <td><label data-bind-transfer="PuntoVenta"></label></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>
                            <br />
                            <center>
                                <button type="button" class="btn transaction-button prev-step"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> Anterior</button>
                                <button id="btnGuardar" type="button" class="btn transaction-button btn_saveTransaction"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                                <button id="btnCerrar04" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                                <button id="btnConstancia" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-list-alt"></span> Constancia</button>
                            </center>
                            <br />
                        </div>
                    </div>

                </div>
                <div id="divErrorAlert" class="alert alert-transaction alert-danger" role="alert">
                    <button type="button" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <strong><span id="lblErrorMessage"></span></strong>
                </div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" id="hndHistoryETA" value="" />
<input type="hidden" id="hndValidateETA" value="0" />

<style>
    .modal-open {
        overflow: scroll;
    }
</style>


@Scripts.Render("~/bundles/steps")
@*<script src="~/Areas/Transactions/Content/Lib/Steps.js"></script>*@
