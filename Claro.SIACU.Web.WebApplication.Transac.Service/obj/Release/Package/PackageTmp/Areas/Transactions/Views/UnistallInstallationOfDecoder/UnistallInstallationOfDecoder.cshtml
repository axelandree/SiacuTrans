    <script src="~/Areas/Transactions/Scripts/UnistallInstallationOfDecoder/UnistallInstallationOfDecoder.js"></script>*@
@Scripts.Render("~/bundles/Transactions/HFC/UnistallInstallationOfDecoder")
@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
@Scripts.Render("~/bundles/Transactions/ReadRecordPDF/ReadRecord")