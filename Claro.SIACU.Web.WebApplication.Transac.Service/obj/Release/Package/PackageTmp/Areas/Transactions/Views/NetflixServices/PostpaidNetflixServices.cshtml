@model Claro.SIACU.Web.WebApplication.Transac.Service.Areas.Transactions.Models.Postpaid.NetflixServicesModel
@{
    Layout = null;
}
<script type="text/javascript">
    var strEstadoContratoInactivo = "@Model.strEstadoContratoInactivo";
    var strEstadoContratoSuspendido = "@Model.strEstadoContratoSuspendido";
    var strEstadoContratoReservado = "@Model.strEstadoContratoReservado";
    var strMsjEstadoContratoInactivo = "@Model.strMsjEstadoContratoInactivo";
    var strMsjServicioContrato = "@Model.strMsjServicioContrato";
    var strMsjEstadoContratoReservado = "@Model.strMsjEstadoContratoReservado";
</script>
<div id="INTNetflixServicePostpaid" class="container-fluid">
    <div class="scrollbudy">
        <div class="panel claro-panel-primary">
            @*<center>REENVIO DE CORREO ELECTRONICO Y SMS PARA REGISTRO EN NETFLIX</center>*@
            <br />
            <div class="panel-body">
                <div class="container-fluid">
                    <div class="col-md-2">
                    </div>
                    <div class="col-md-8">
                        <div id="DataCustomerB" class="panel-group" role="tablist" aria-multiselectable="true">
                            <div class="panel claro-panel-primary">
                                <div id="personalDataHead" role="tab" class="panel-heading panel-info">
                                    <span class="glyphicon glyphicon-user"></span>
                                    <a data-toggle="collapse" href="#personalDataCollapse" aria-expanded="true" aria-controls="personalDataCollapse" class="collapsed transaction-ref-color" style="color: #020202 !important;"> Datos de Cliente</a>
                                </div>
                                <div class="panel-body panel-collapse collapse in" id="personalDataCollapse" role="tabpanel" aria-labelledby="personalDataHead">
                                    <div class="container-fluid">
                                        <div class="row">
                                            <form id="frmNetflixService" class="form-horizontal">
                                                <input type="hidden" id="hidIdInteraccion" />
                                                <div class="form-group" id="divNameCli">
                                                    <label class="col-sm-4 control-label" for="txtNombres">Nombres<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-5">
                                                        <input id="txtNombres" name="nmtxtNombres" class="form-control input-sm" disabled="disabled" />
                                                    </div>
                                                </div>
                                                <div class="form-group" id="divApeTobe">
                                                    <label class="col-sm-4 control-label cltobe" for="txtApellidos">Apellidos<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-6 cltobe">
                                                        <input id="txtApellidos" name="nmtxtApellidos" class="form-control input-sm" disabled="disabled" />
                                                    </div>
                                                </div>
                                                <div class="form-group" id="divNameC">
                                                    <label class="col-sm-4 control-label" for="txtEmail">Correo Electronico<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-5">
                                                        <input id="txtEmail" name="nmtxtEmail" class="form-control input-sm" disabled="disabled" />
                                                    </div>
                                                    <div class="col-sm-1" style="width: 15px;"><input id="chkCorreo" type="checkbox" class="form-check-input" /></div>
                                                    <div class="col-sm-1"><label>Seleccionar</label></div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="txtLinea" class="col-sm-4 control-label">Línea/Contrato<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-3">
                                                        <input id="txtLinea" name="nmtxtLinea" class="form-control input-sm" disabled="disabled" />
                                                    </div>
                                                    <div class="col-sm-1" style="width: 35px;"><input id="chkLinea" type="checkbox" class="form-check-input" /></div>
                                                    <div class="col-sm-1"><label>Seleccionar</label></div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label" for="cboCacDac">CAC/DAC:<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-4 error-input"><select id="cboCacDac" name="nmcboCacDac" class="form-control select-list"></select></div>
                                                    <div class="col-sm-4"><input type="text" class="form-control input-sm" id="txtDireccion" style="visibility:hidden" /></div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label" for="txtNotas">Notas:<span class="text-right label-red-service"> (*):</span></label>
                                                    <div class="col-sm-7"><textarea id="txtNotas" class="form-control" placeholder="Agregar Notas" rows="4" maxlength="3800"></textarea></div>
                                                    <br />
                                                </div>

                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="container-fluid">
                                <br/>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <center>
                                            <button id="btnGuardar" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                                            <button id="btnCerrar" type="button" class="btn transaction-button"><span class="glyphicon glyphicon-home"></span> Cerrar</button>
                                        </center>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<script src="~/Areas/Transactions/Scripts/NetflixServices/PostpaidNetflixServices.js"></script>