
@{
    ViewBag.Title = "HFCAdditionalPointsConstPrint";
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="HFCAdditionalPointsConstPrint" class="container">
    <header>
        <nav class="navbar btn-danger navbar-fixed-top">
            <div class="containe">
                <div style="text-align: center;">
                    <h4><strong><span id="spnMainTitle">Solicitud de Instalación de Puntos Adicionales</span></strong></h4>
                </div>
            </div>
        </nav>
    </header>
    <br />
    <div class="row" style="padding:15px; margin:15px;">
        <div class="panel panel-danger">
            <div class="">

                <div class="row">
                    <div class="col-sm-12 text-center text-uppercase">
                        <label id="lblSubTitle"></label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="table-responsive">
                            <table class="table table-striped">
                                <tr>
                                    <td><strong>Centro Atención:</strong></td>
                                    <td><label id="lblAttentionCenter"></label></td>

                                    <td><strong>Fecha (dd/mm/aaaa):</strong></td>
                                    <td id="lblDate"></td>
                                </tr>
                                <tr>
                                    <td><strong>Cliente / Razón Social:</strong></td>
                                    <td><label id="lblBusinessName"></label></td>

                                    <td><strong>Caso / Interacción:</strong></td>
                                    <td>
                                        <label id="lblInteration"></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>Contacto:</strong></td>
                                    <td>
                                        <label id="lblContact"></label>
                                    </td>

                                    <td><strong>Contrato:</strong></td>
                                    <td>
                                        <label id="lblContract"></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>Tipo Doc. Identidad:</strong></td>
                                    <td>
                                        <label id="lblDocumentType"></label>
                                    </td>

                                    <td><strong></strong></td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td><strong>Nro. Doc:</strong></td>
                                    <td>
                                        <label id="lblNumberDocument"></label>
                                    </td>

                                    <td><strong></strong></td>
                                    <td></td>
                                </tr>
                            </table>
                        </div>

                    </div>
                  

                </div>

            </div>

        </div>
        <div class="panel panel-danger">
            <div class="">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="table-responsive">
                            <table style="width:100%;" class="table table-striped">
                                <tr>
                                    <td><strong>Tipo de Trabajo:</strong></td>
                                    <td>
                                        <label id="lblJobType"></label>
                                    </td>
                                </tr>
                                <tr id="trServicesType">
                                    <td><strong>Tipo de Servicio:</strong></td>
                                    <td>
                                        <label id="lblServiceType"></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>SOT:</strong></td>
                                    <td>
                                        <label id="lblSot"></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>Motivo SOT:</strong></td>
                                    <td>
                                        <label id="lblMotiveSot"></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>Dirección de Instalación: </strong></td>
                                    <td>
                                        <label id="lblInstallationAddress"></label>
                                    </td>
                                </tr>

                                <tr>
                                    <td><strong>Fidelidad: </strong></td>
                                    <td>
                                        <label id="lblFidelidad"></label>
                                    </td>
                                </tr>

                                <tr>
                                    <td><strong>Monto: </strong></td>
                                    <td>
                                        <label id="lblMonto"></label>
                                    </td>
                                </tr>

                                <tr>
                                    <td><strong>Teléfono de Referencia:</strong></td>
                                    <td id="lblPhoneReference"></td>
                                </tr>
                                <tr>
                                    <td><strong>Fecha Compromiso:</strong></td>
                                    <td id="lblCommitmentDate"></td>
                                </tr>
                                <tr>
                                    <td><strong>Cantidad Anexos:</strong></td>
                                    <td id="lblAttachmentsNumber"></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-danger">
            <div class="">
                <div class="row">
                    <div class="col-sm-12">
                        <label id="lblClosing" style="text-align:justify; ">
                        </label>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-danger">
            <div class="">
                <div class="row">
                    <div class="col-sm-12">
                        <p style="text-align:justify;">
                            Por la presente doy fe que los datos personales antes consignados son verdaderos y autorizo a América Móvil Perú SAC, que en caso alguno de los datos proporcionados no sean válidos o no coincidan con los registros oficiales, se dé por inválida esta solicitud.
                        </p>
                    </div>
                </div>
            </div>
        </div>


        <div style="float:left; width:80px; height:234px;">
            <!-- en blanco-->
        </div>
        <div style="float:left; width:180px; height:234px;">
            <div style="height:120px; border-bottom-style:solid; border-bottom-color:Black;"></div>
            <div style="text-align:center; padding-top:5px;">
                <label style="font-size:11px;">
                    Firma del Cliente
                </label>
                <br />
                <input id="btnImprimir" type="button" class="Boton" style="width:100px; margin-top:15px;" value="Imprimir" />
            </div>
        </div>
        <div style="float:left; width:80px; height:234px;">
            <!-- en blanco central-->
        </div>
        <div style="float:left; width:180px; height:234px;">
            <div style="height:120px; border-bottom-style:solid; border-bottom-color:Black;"></div>
            <div style="text-align:center; padding-top:5px;">
                <label style="font-size:11px;">
                    Firma del Asesor
                </label>
                <br />
                <input id="btnCerrar" type="button" class="Boton" style="width:100px; margin-top:15px;" value="Cerrar" />
            </div>
        </div>
        <div style="float:left; width:80px; height:234px;">
            <!-- en blanco-->
        </div>


        <div class="panel panel-danger">
            <div class="">
                <div class="row">
                    <div class="col-sm-12">
                        <label id="lblMessage" style="text-align:justify; color:red; font-weight:bold;">
                        </label>
                    </div>
                </div>
            </div>
        </div>
        <div>

            
        </div>


    </div>
</div>

<script>
    var strClosing = "@ViewData["lblClosing"]";
    var CaseId =    "@ViewData["CaseID"]";   
</script>

@section scripts
{
    @Scripts.Render("~/bundles/Transactions/AdditionalPoints/HFCAdditionalPointsConstPrint")
}
