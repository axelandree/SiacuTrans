
@{
    Layout = null;
    // Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
}
 
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="HFCAdditionalPoints" class="container-fluid">
    <div class="scrollbudy">
        <div class="col-md-3">
            @Html.Partial("~/Areas/Transactions/Views/Shared/ViewPartial/HFCLeftPanelCustomer.cshtml")
        </div>
        <div class="col-md-9">

            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    Listado de Equipos Asociados al Cliente
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="table-responsive">
                                <table style="width:100%;" id="tb_ProductDetailts" class="table  table-striped table-hover table-responsive claro-modal-tabla table-bordered">
                                    <thead>
                                        <tr class="claro-bg-primary">
                                            <th>Cod.&nbsp;Material</th>
                                            <th>Cod.&nbsp;SAP</th>
                                            <th>Num.&nbsp;Serie</th>
                                            <th>Mac&nbsp;Ad.</th>
                                            <th>Desc.&nbsp;Material</th>
                                            <th>Tipo&nbsp;Equipo</th>
                                            <th>Código/Id&nbsp;Producto</th>
                                            <th>Modelo</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                 
                            </div>
                        </div>
                    </div>

                </div>
            </div>


            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    Datos de la Transacción
                </div>
                <div class="panel-body">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label for="cboJobTypes" class="col-sm-2 control-label">Tipo de Trabajo</label>
                            <div class="col-sm-4">
                                <select id="cboJobTypes" class="form-control input-sm" disabled></select>
                            </div>
                            <label for="cboMotivoSot" class="col-sm-2 control-label">Motivo SOT</label>
                            <div class="col-sm-4">
                                <select id="cboMotivoSot" class="form-control input-sm"></select>
                            </div>
                        </div>
                        <div id="tr_SubWorkType" class="form-group">
                            <label for="cboSubJobTypes" class="col-sm-2 control-label">Sub Tipo de Trabajo</label>
                            <div class="col-sm-4">
                                <select id="cboSubJobTypes" class="form-control input-sm" disabled="disabled">
                                    <option value="-1">Seleccionar</option>
                                </select>
                            </div>
                        </div>
                        <div id="tr_ServicesType" class="form-group">
                            <label for="cboServicesType" class="col-sm-2 control-label">Tipo de Servicio</label>
                            <div class="col-sm-4">
                                <select id="cboServicesType" class="form-control input-sm">
                                    <option value="-1">Seleccionar</option>
                                </select>
                            </div>
                            <label for="cboAttachedQuantity" class="col-sm-2 control-label">Cantidad Anexos</label>
                            <div class="col-sm-4">
                                <select id="cboAttachedQuantity" class="form-control input-sm">
                                    <option value="-1">Seleccionar</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="chk_Fidelidad" class="col-sm-2 control-label">Fidelizar</label>
                            <div class="col-sm-1">
                                <div class="checkbox text-center">
                                    <label>
                                        <input id="chk_Fidelidad" type="checkbox">
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-5">
                                <label for="txt_monto" class="col-sm-7 control-label">Costo de Instalación (Con IGV)</label>
                                <div class="col-sm-5">
                                    <input type="text" id="txt_monto" class="form-control input-sm text-right" readonly />
                                </div>


                            </div>
                        </div>
                    </form>

                    @Html.Partial("~/Areas/Transactions/Views/Shared/ViewPartial/Schedule.cshtml",
                            new ViewDataDictionary {
                                { "Text_Id", "txt_dDateProgramming" },
                                { "Select_Id", "cboSchedule" },
                                { "Button_Id", "btnValidateSchedule" },
                                { "Combo_JobTypes" , "cboJobTypes"},
                                { "Combo_SubJobType" , "cboSubJobTypes"},
                                { "Input_Size" , "input-xs"},
                                { "strDateServer", @ViewData["strDateServer"] },
                                { "strDateNew", @ViewData["strDateNew"] },
                                { "Button_Save", "btn_saveTransaction"}
                            })

                    <div class="form-horizontal">
                        <div class="form-group">
                            <label for="lblHCFNroSot" class="col-sm-2 control-label">Número de SOT</label>
                            <label class="col-sm-2 control-label">
                                <span id="lblHCFNroSot" class="badge">0</span>
                            </label>
                        </div>
                        <div class="form-group">
                            <label for="txtNote" class="col-sm-2 control-label">Notas</label>
                            <div class="col-sm-9">
                                <textarea id="txtNote" class="form-control"></textarea>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="chk_SendMail" class="col-sm-2 control-label">Enviar Email</label>
                            <div class="col-sm-1">
                                <div class="checkbox text-center">
                                    <label>
                                        <input id="chk_SendMail" type="checkbox">
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <input type="text" id="txt_SendMail" class="form-control input-sm" />
                            </div>
                            <label for="txt_phone_references" class="col-sm-2 control-label">Teléfono Referencia</label>
                            <div class="col-sm-4">
                                <input type="text" id="txt_phone_references" class="form-control input-sm" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="cboCacDac" class="col-sm-2 control-label">Punto de Atención</label>
                            <div class="col-sm-4">
                                <select class="form-control input-sm" id="ddlCACDAC"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <center>
                                <button class="btn transaction-button btn_saveTransaction" id="btnGuardar" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>
                                <button class="btn transaction-button" id="btnConstancia" type="button"><span class="glyphicon glyphicon-file"></span> Constancia</button>
                                <button class="btn transaction-button" id="btnCerrar" type="button"><span class="glyphicon glyphicon-remove"></span> Cerrar</button>
                            </center>
                        </div>
                    </div>
                </div>
            </div>

            </div>

        <div id="ModalLoading" style="display: none;">
            <span>
                <img src="~/Images/loading2.gif" width="25" height="25" /> Cargando ....
            </span>
        </div>


    </div>
</div>
 

 <style>
    .modal-open {
        overflow: scroll;
    }
</style>

 
<script src="~/Areas/Transactions/ContentTransac/lib/steps.js"></script>
<script src="~/Areas/Transactions/Scripts/ReadRecordPdf/ReadRecord.js"></script>
@Scripts.Render("~/bundles/Transactions/AdditionalPoints/HFCAdditionalPoints")@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")