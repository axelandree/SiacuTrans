@{
    Layout = null;
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="BilledCallDetail" class="container-fluid">
    <div class="row">

        <div class="col-md-3">

            <div id="PostpaidDataCustomer" class="panel claro-panel-primary ">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-user"></span>
                    <a data-toggle="collapse" role="tab" href="#DatosClientecollapse" class="collapsed transaction-ref-color">
                        Datos del Cliente
                    </a>
                </div>
                <div id="DatosClientecollapse" class="">
                    <table class="table table-striped table-responsive table-condensed">
                        <tr>
                            <td><strong>Contrato</strong></td>
                            <td><span id="lblContract"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Tipo de Cliente</strong></td>
                            <td><span id="lblTypeCustomer"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Contacto</strong></td>
                            <td><span id="lblContactCode"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Cliente</strong></td>
                            <td><span id="lblCustomerName"></span></td>
                        </tr>
                        <tr>
                            <td><strong>DNI/RUC</strong></td>
                            <td><span id="lblIdentificationDocument"></span></td>
                        </tr>
                        <tr>
                            <td>Plan Actual</td>
                            <td><span id="lblPlan"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Fecha de Activación</strong></td>
                            <td><span id="lblDateActivation"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Ciclo de Facturación</strong></td>
                            <td><span id="lblCycleBilling"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Representante Legal</strong></td>
                            <td><span id="lblReprLegal"></span></td>
                        </tr>
                        <tr>
                            <td><strong>Código de Cliente</strong></td>
                            <td><span id="lblCustomerCode"></span></td>
                        </tr>

                    </table>
                </div>
            </div>
        </div>
        <div id="content" class="col-md-9 transaction-content">
            <div class="panel claro-panel-primary">
                <div class="panel-heading">
                    Detalle de Llamadas Salientes Facturado Postpago
                </div>
                <div class="panel-body">
                    <form id="tbDatoTransaccion" class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-7 control-label">Periodo(s) de facturación solicitado(s)</label>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Mes</label>

                            <div class="col-sm-2">

                                <select class="form-control  input-sm" id="ddlMonths"></select>
                            </div>
                            <label class="col-sm-2 control-label">Año</label>
                            <div class="col-sm-2">
                                <select class="form-control  input-sm" id="ddlYears"></select>

                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Enviar por Correo</label>
                            <div class="col-sm-1 checkbox">
                                <label>
                                    <input type="checkbox" id="chkSendMail">
                                </label>
                            </div>
                            <div id="dtxtSendMail" class="col-sm-5">
                                <input style="display: none" id="txtSendMail" type="text" class="form-control input-sm" placeholder="Email">
                            </div>

                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Punto de Atención</label>
                            <div class="col-sm-2">
                                <select class="form-control input-sm" id="ddlCACDAC"></select>
                            </div>
                            <div class="col-sm-1">
                                <button id="btnSearch" type="button" class="btn transaction-button btn-sm" accesskey="btnBuscar"><span class="glyphicon glyphicon-search">&nbsp;</span>Buscar</button>

                            </div>
                        </div>
                    </form>

                </div>
                <div>

                    <div id="contenedorBilledCallDetail" class="table">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="table-responsive">
                                    <table id="tblBilledCallDetail" class="table table-hover transaction-table table-bordered">
                                        <thead>
                                            <tr class="btn-danger">
                                                <th>Nro</th>
                                                <th>Fecha</th>
                                                <th>Hora</th>
                                                <th>Teléfono Destino</th>
                                                <th>Consumo</th>
                                                <th>Consumo (S/.)</th>
                                                <th>Tipo</th>
                                                <th>Destino</th>
                                                <th>Operador</th>
                                            </tr>
                                        </thead>

                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="contenedorBilledCallDetail" class="table">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="table-responsive" style="overflow-x: auto">
                                    <table id="tblBilledCallDetail_1" class="table table-hover transaction-table table-bordered">
                                        <thead>
                                            <tr class="transaction-header-table">
                                                <th>Nro</th>
                                                <th>Fecha</th>
                                                <th>Hora</th>
                                                <th>Teléfono Destino</th>
                                                <th>Consumo</th>
                                                <th>Costo (S/.)</th>
                                                <th>Plan</th>
                                                <th>Tipo</th>
                                                <th>Destino</th>
                                                <th>Operador</th>
                                                <th>Horario</th>
                                                <th>Saldo (S/.)</th>
                                            </tr>
                                        </thead>
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="myModalLoad" style="display:none;">
                        <span>
                            <img src="~/Images/loading2.gif" width="25" height="25" />  Cargando ....
                        </span>
                    </div>
                </div>
                <br />
                <br />
                <table id="tbDatoCliente" class="table table-condensed">
                    <tr>
                        <td>
                            <label class="control-label" id="litTotal">Total General Minutos(hh:mm:ss): </label>
                        </td>
                        <td>
                            <span id="lblTotal"></span>
                        </td>
                        <td>
                            <label><b>&#124;</b></label>
                        </td>

                        <td>
                            <label class="control-label" id="litTotalSMS">Total General SMS: </label>
                        </td>
                        <td>
                            <span id="lblTotalSMS"></span>
                        </td>
                        <td>
                            <label><b>&#124;</b></label>
                        </td>
                        <td>
                            <label class="control-label" id="litTotalMMS">Total General MMS: </label>
                        </td>
                        <td>
                            <span id="lblTotalMMS"></span>
                        </td>
                    </tr>
                    <tr>
                        <td> <label class="control-label" id="litTotalRegistro">Total Registros: </label></td>
                        <td><span id="lblTotalRegistro"></span></td>
                        <td>
                            <label><b>&#124;</b></label>
                        </td>
                        <td><label class="control-label" id="litCargoFinal">Total Consumo/Saldo: </label></td>
                        <td><span id="lblCargoFinal"></span></td>
                        <td>
                            <label><b>&#124;</b></label>
                        </td>
                        <td><label class="control-label" id="litGPRS">Total General GRPS: </label></td>
                        <td><span id="lblTotalGPRS"></span></td>

                    </tr>
                </table>
                <br />
                <div>
                    <div class="panel-body">
                        <form class="form-horizontal ">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Notas</label>
                                <div class="col-sm-10">
                                    <textarea id="txtNote" class="form-control" rows="3"></textarea>
                                </div>
                            </div>
                        </form>
                    </div>
            </div>
            
                    <div>
                        <center>
                            <form class="form-inline ">
                                <div style="text-align: left">
                                    <label id="lblResultMessag" class="alert-danger"></label>
                                </div>
                                <div class="form-group">
                                    <button class="btn transaction-button btn-sm" id="btnSave" type="button"><span class="glyphicon glyphicon-floppy-disk">&nbsp;</span>Guardar</button>
                                </div>
                                <div class="form-group">
                                    <button class="btn transaction-button btn-sm" id="btnPrint" type="button"><span class="glyphicon glyphicon-print">&nbsp;</span>Imprimir</button>
                                </div>
                                <div class="form-group">
                                    <button class="btn transaction-button btn-sm" id="btnExport" type="button"><span class="glyphicon glyphicon-export">&nbsp;</span>Exportar</button>
                                </div>
                                <div class="form-group" id="loo">
                                    <button class="btn transaction-button btn-sm" id="btnConstancy" type="button"><span class="glyphicon glyphicon-file">&nbsp;</span>Constancia</button>
                                </div>
                                <div class="form-group">
                                    <button class="btn transaction-button btn-sm" id="btnSendEmail" type="button"><span class="glyphicon glyphicon-send">&nbsp;</span>Enviar</button>
                                </div>
                                <div class="form-group">
                                    <button class="btn transaction-button btn-sm" id="btnClose" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                                </div>
                            </form>
                        </center>
                    <br />
                </div>
                    </div>
                </div>
            </div>
</div>
<input id="hidOpcion" type="hidden" name="hidOpcion" />
<input id="hidCodOpcion" type="hidden" name="hidCodOpcion" />
<input id="hidAccion" type="hidden" name="hidAccion" />
@*@section scripts{
    <script src="~/bundles/Transactions/ReadRecordPDF/ReadRecord"></script>
    }*@
@Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
@Scripts.Render("~/bundles/Transactions/Postpaid/BilledOutCallDetail")
@Scripts.Render("~/bundles/Transactions/ReadRecordPDF/ReadRecord")