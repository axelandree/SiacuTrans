            <div class="panel-heading" role="tab">
                Cable
            </div>
            <table id="tblChooseCoreServicesByPlanCable" class="table table-hover claro-modal-tabla table-bordered">
                <thead>
                    <tr class="transac-table-header">
                        <th class="col-md-1"></th>
                        <th class="col-md-9">Nombre del Servicio</th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
