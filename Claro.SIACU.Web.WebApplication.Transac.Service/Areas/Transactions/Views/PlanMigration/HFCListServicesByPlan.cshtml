@using System.Web.Optimization;
@model Claro.SIACU.Web.WebApplication.Transac.Service.Areas.Transactions.Models.HFC.MigrationModel
@{

}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="modal-content">
    <div id="RentalEquipment">
        <div class="panel panel-info">
            <div id="DatosBcollapse" class="panel-collapse collapse in table-responsive">
                <table id="tblResumenRE" class="table  table-striped table-hover table-responsive claro-modal-tabla ">
                    <thead class="claro-bg-primary">
                        <tr>
                            <th class="col-sm-1">Sel</th>
                            <th class="col-sm-6">Descripción del Servicio</th>
                            <th></th>
                            <th>Equipo Asociado</th>
                            <th>Cantidad</th>
                            <th>Desc. Equipo</th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th class="col-sm-5" data-toggle="tooltip" title="(S/.)">Cargo Fijo(S/ inc. IGV)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.ServicesByPlan != null)
                        {
                            foreach (var item in Model.ServicesByPlan)
                            {
                                if (!Model.SelectedServices.Contains(item.CodServSisact))
                                {
                                <tr>
                                    <td></td>
                                    <td>@item.DesServSisact</td>
                                    <td>@item.CF </td>
                                    <td>@item.Equipment</td>
                                    <td>@(item.CantEquipment ?? "0")</td>
                                    <td>@item.Dscequ</td>
                                    <td>@item.CodServSisact</td>
                                    <td>@item.ServiceType</td>
                                    <td>@item.Sncode</td>
                                    <td>@item.Spcode</td>
                                    <td>@item.CodGroupServ</td>
                                    <td>@item.Codtipequ</td>
                                    <td>@item.Tipequ</td>
                                    <td>@item.Tmcode</td>
                                    <td>@item.CodPlanSisact</td>
                                    <td>@item.IDEquipment </td>
                                    <td>@item.CfWithIgv</td>
                                </tr>
                            }
                                
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/Areas/Transactions/Scripts/PlanMigration/HFCRentalEquipment.js"></script>


