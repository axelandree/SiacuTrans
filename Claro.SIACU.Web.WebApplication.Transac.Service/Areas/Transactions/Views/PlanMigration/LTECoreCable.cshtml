@using System.Web.Optimization;
@model Claro.SIACU.Web.WebApplication.Transac.Service.Areas.Transactions.Models.LTE.LteMigrationModel
@{

}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="modal-content">
    <div id="CoreCable">
        <div class="panel panel-info">
            <div class="panel-heading claro-modal-contenido-titulo-panel">
                <h4 class="panel-title claro-modal-contenido-titulo-texto">
                    <a data-toggle="collapse" href="#DatosBcollapse">SERVICIOS CABLE</a>
                </h4>
            </div>

            <div id="DatosBcollapse" class="panel-collapse collapse in table-responsive">
                <table id="tblCoreCable" class="table  table-striped table-hover table-responsive claro-modal-tabla ">
                    <thead class="claro-bg-primary">
                        <tr>
                            <th class="col-sm-1">Sel</th>
                            <th class="col-sm-6">Descripción del Servicio</th>
                            <th class="col-sm-5">Cargo Fijo (S/.)</th>
                            <th>Equipo Asociado</th>
                            <th>Cantidad</th>
                            <th>Desc. Equipo</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.ServicesByPlan != null)
                        {
                            foreach (var item in Model.ServicesByPlan)
                            {
                                <tr>
                                    <td></td>
                                    <td>@item.DES_SERV_SISACT</td>
                                    <td>@item.CF </td>
                                    <td>@item.EQUIPO</td>
                                    <td>@(item.CANT_EQUIPO ?? "0")</td>
                                    <td>@item.DSCEQU</td>
                                    <td>@item.COD_SERV_SISACT</td>
                                </tr>
                            }

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/Areas/Transactions/Scripts/PlanMigration/LTECoreCable.js"></script>

