@{
    Layout = "~/Areas/Transactions/Views/Shared/_Layout.cshtml";
    var contractId = ViewBag.ContractId; // ASIGNAR A CAJA DE TEXTO
}
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div class="container">
    <div class="row">
        <div class="col-md-3">
            @*<div id="PostpaidDataCustomer" class="panel claro-panel-primary ">
