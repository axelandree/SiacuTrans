            </div>*@
            <div id="DataCustomer" class="panel-group" role="tablist" aria-multiselectable="true">
                <div class="panel claro-panel-primary">
                    <div id="customerDataHead" role="tab" class="panel-heading">
                        <span class="glyphicon glyphicon-user"></span>
                        <a data-toggle="collapse" href="#customerDataCollapse" aria-expanded="true" data-parent="#DataCustomer" aria-controls="customerDataCollapse" class="collapsed transaction-ref-color"> Datos del Cliente</a>
                    </div>
                    <div class="panel-body panel-collapse collapse in" id="customerDataCollapse" role="tabpanel" aria-labelledby="customerDataHead">
                        <table class="table table-condensed">
                            <tbody>
                            <tr>
                                <td class="col-md-3">Contrato:</td>
                                <td><label id="lblContract"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">
                                    Tipo Cliente:
                                </td>
                                <td><label id="lblTypeCustomer"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Cliente:</td>
                                <td><label id="lblCustomerName"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Contacto:</td>
                                <td><label id="lblContact"></label></td>

                            </tr>
                            <tr>
                                <td class="col-md-3">DNI/RUC:</td>
                                <td><label id="lblIdentificationDocument"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Plan Actual:</td>
                                <td><label id="lblPlanName"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Fecha de Activación:</td>
                                <td><label id="lblDateActivation"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Ciclo de Facturación:</td>
                                <td><label id="lblCycleFacture"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Límite de Crédito:</td>
                                <td><label id="lblLimitCredit"></label></td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Representante Legal:</td>
                                <td><label id="lblReprLegal"></label> </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Codigo de Cliente:</td>
                                <td><label id="lblCustomerId"></label> </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Doc Repres. Legal:</td>
                                <td><label id="lblDocReprLegal"></label></td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                    <div id="addressDataHead" role="tab" class="panel-heading">
                        <span class="glyphicon glyphicon-home"></span>
                        <a data-toggle="collapse" href="#addressDataCollapse" aria-expanded="false" data-parent="#DataCustomer" aria-controls="addressDataCollapse" class="collapsed transaction-ref-color"> Dirección de Facturación</a>
                    </div>
                    <div class="panel-body panel-collapse collapse" id="addressDataCollapse" role="tabpanel" aria-labelledby="addressDataHead">
                        <table class="table table-condensed">
                            <tr>
                                <td class="col-md-3">Dirección</td>
                                <td class="col-md-3">
                                    <label id="lblAddress"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Referencia</td>
                                <td class="col-md-3">
                                    <label id="lblAddressNote"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Departamento</td>
                                <td class="col-md-3">
                                    <label id="lblDepartamento"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">País</td>
                                <td class="col-md-3">
                                    <label id="lblPais"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Distrito</td>
                                <td class="col-md-3">
                                    <label id="lblDistrito"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Provincia</td>
                                <td class="col-md-3">
                                    <label id="lblProvincia"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Código de Plano</td>
                                <td class="col-md-3">
                                    <label id="lblCodePlans"></label>
                                </td>
                            </tr>
                            <tr>
                                <td class="col-md-3">Código de Ubigeo</td>
                                <td class="col-md-3">
                                    <label id="lblUbigeo"></label>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-9">
            <div id="" class="panel claro-panel-primary">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-book"></span>
                    <a data-toggle="collapse" href="#Contratcollapse" style="color: #fff">
                        Datos de la Transacción
                    </a>
                </div>
                <div id="Contratcollapse" class="">
                    <div id="tbDatoTransaccion" class="form-horizontal">
                        <br/>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Contrato:</label>
                            <div class="col-sm-3">
                                <input type="text" id="txtIdContract" class="form-control" value="@contractId" disabled="disabled"/>
                            </div>

                            <label class="col-sm-2 control-label">Cuenta:</label>
                            <div class="col-sm-3">
                                <input type="text" id="txtAccount" class="form-control" disabled="disabled" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Estado:</label>
                            <div class="col-sm-3">
                                <select id="cboState" class="form-control"></select>
                            </div>
                            <label class="col-sm-2 control-label">Teléfono:</label>
                            <div class="col-sm-3">
                                <input type="text" id="txtTelephone" class="form-control" disabled="disabled" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Fecha de programación:</label>
                            <div class="col-sm-3">
                                <div class="input-group">
                                    <input type="text" id="txtFromDate" class="form-control"/>
                                    <label class="input-group-addon btn" for="txtFromDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>

                            <label class="col-sm-2 control-label">a</label>
                            <div class="col-sm-3">
                                <div class="input-group">
                                    <input type="text" id="txtToDate" class="form-control"/>
                                    <label class="input-group-addon btn" for="txtToDate">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Tipo de transacción:</label>
                            <div class="col-sm-3">
                                <select id="cboTypeTransaction" class="form-control"></select>
                            </div>

                            <label class="col-sm-2 control-label">Código de Interacción: </label>
                            <div class="col-sm-3">
                                <input type="text" id="txtInteractionCode" class="form-control"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Asesor: </label>
                            <div class="col-sm-3">
                                <input type="text" id="txtAdviser" class="form-control"/>
                            </div>

                            <label class="col-sm-2 control-label">Punto de atención: </label>
                            <div class="col-sm-3">
                                <select id="cboAtentionLocal" class="form-control"></select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>                                            
            <div>
                <div id="divProgramTask" class="table">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive" style="overflow-x: auto">
                                <table id="tblProgramTask" class="table  table-striped table-hover table-responsive claro-modal-tabla table-bordered">
                                    <thead class="btn-danger">
                                    <tr>
                                        <th>Contrato</th>
                                        <th>Código Cliente</th>
                                        <th>Fecha Programación</th>
                                        <th>Fecha Registro</th>
                                        <th>Fecha Ejecución</th>
                                        <th>Estado</th>
                                        <th>Descripción Servicio</th>
                                        <th>Cuenta</th>
                                        <th>Tipo Servicio</th>
                                        <th>Acción</th>
                                    </tr>
                                    </thead>
                                    <tbody id="tbody">

                                    </tbody>
                                </table>
                                <div id="myModalLoad" style="display: none;">
                                    <span>
                                        <img src="~/Images/loading2.gif" width="25" height="25"/> Cargando ....
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <center>
                    <form class="form-inline ">
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnSearch" type="button"><span class="glyphicon glyphicon-floppy-disk">&nbsp;</span>Consultar</button>
                        </div>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnExport" type="button"><span class="glyphicon glyphicon-print">&nbsp;</span>Exportar</button>
                        </div>
                        <div class="form-group">
                            <button class="btn transaction-button btn-sm" id="btnClose" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                        </div>
                    </form>
                </center>
            </div>
        </div>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/bundles/Transactions/Auth/AuthUser")
    @Scripts.Render("~/bundles/Transactions/LTE/LteProgramTask")
}



