
<style type="text/css">
    fieldset.scheduler-border {
        border: solid 2px #DDD !important;
        padding: 0 10px 10px 10px;
        border-bottom: none;
    }

    legend.scheduler-border {
        width: auto !important;
        border: none;
        font-size: 16px;
    }
</style>
<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div id="ContentHistoryRentalUser">
    <div class="panel panel-default" style="margin-left:15px; margin-right:15px">
        <div class="panel">
            <div class="row">
                <div class="col-md-1"></div>
                <div class="col-md-2"> <label for="FechaDesde" class="col-form-label">Fecha desde:</label> <span class="glyphicon glyphicon-calendar"></span> </div>
                <div class="col-md-2"><input type="text" class="form-control" id="FechaDesde" value="" placeholder="(dd/mm/yyyy)" name="start" /></div>
                <div class="col-md-2"> <label for="FechaHasta" class="col-form-label">Fecha Hasta:</label> <span class="glyphicon glyphicon-calendar"></span></div>
                <div class="col-md-2"><input type="text" class="form-control" id="FechaHasta" placeholder="(dd/mm/yyyy)" name="start" value="" /></div>
                <div class="col-md-2">
                    <button style="padding-left: 35px; padding-right: 35px; margin: 5px; " class="btn btn-danger btn-sm" id="btnBuscarAlquiler" type="button">Buscar</button>
                </div>
                <div class="col-md-1"></div>
            </div>
        </div>
        <div class="panel-body">

            <div class="row" style="margin-left:1px; margin-right:1px">
                <div class="well">
                    <table id="tbHistoryRentalUser" class="table  table-striped table-hover table-responsive claro-modal-tabla table-bordered">
                        <thead class="claro-bg-primary">
                            <tr>
                                <th style="text-align:center">Titulo</th>
                                <th style="text-align: center">IP Usuario</th>
                                <th style="text-align: center">Ultimo tiempo de visualizaciòn</th>
                                <th id="columPendiente" style="text-align: center">Tiempo Maximo de visualizacion</th>
                                <th style="text-align: center">Fecha de Alta</th>
                                <th style="text-align: center">Fecha de expiracion</th>
                                <th style="text-align: center">Precio</th>
                                <th style="text-align: center">Medio de Pago</th>
                                @*<th style="text-align: center">Acciones</th>*@
                            </tr>
                        </thead>
                        <tbody id="tbHistoryRentalUserBody"></tbody>
                    </table>
                </div>
            </div>




            <div class="row">
            </div>
        </div>
    </div>

    @* wbc - inicio *@

    <input type="hidden" id="hdidTipoDocumentoTitular" value="0" />
    <input type="hidden" id="hdidTipoDocumentoContacto" value="0" />
    <input type="hidden" id="hdidTipoCliente" value="" />
    <input type="hidden" id="hdidTipoLinea" value="" />
    <input type="hidden" id="hdidTipoOrigen" value="" />
    <input type="hidden" id="hdCAC" value="" />
    <input type="hidden" id="hdidCliente" value="0" />
    <input type="hidden" id="hdidContacto" value="0" />
    <input type="hidden" id="hdCustomerIDClaroVideo" value="@ViewBag.CustomerID" />
    @* wbc - fin *@
</div>

@Scripts.Render("~/bundles/Transactions/HistoryRentalUser")
