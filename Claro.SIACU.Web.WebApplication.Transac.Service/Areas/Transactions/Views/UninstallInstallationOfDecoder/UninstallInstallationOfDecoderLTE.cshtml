<style>
    table > tbody > tr > td.border-top {
        border-top: 0px;
    }

    textarea.form-control {
        height: 100px;
    }

    .modalInsDes {
        position: relative;
        border: none;
        bottom: 250px;
        background-color: #000;
        -webkit-border-radius: 10px;
        -moz-border-radius: 10px;
        opacity: .5;
        color: #fff;
        margin-left: 270px;
        margin-right: 270px;
        padding: 10px;
    }

    div#LTEListAddtionalEquipment > div > div {
        margin-bottom: 0 !important;
    }

    body {
        padding-right: 0 !important;
    }

    .table-sortable {
        position: relative;
    }

    .table-sortable .sortable-placeholder {
        height: 37px;
    }

    .table-sortable .sortable-placeholder:after {
        position: absolute;
        z-index: 10;
        content: " ";
        height: 37px;
        background: #f9f9f9;
        left: 0;
        right: 0;
    }

    /*  .v-center {
        min-height: 200px;
        display: flex;
        justify-content: center;
        flex-flow: column wrap;
    }*/

    .progress-bar-info {
        background-color: #da272c;
    }

    .list-footer {
        position: relative;
        display: block;
        border-top-left-radius: 0px;
        border-top-right-radius: 0px;
        border-bottom-left-radius: 4px;
        border-bottom-right-radius: 4px;
        padding: 5px 10px;
        background-color: #ffffff;
        border: 1px solid #dddddd;
    }

    .badge-dec {
        display: inline-block;
        border-radius: 0.25rem;
        line-height: 1;
        padding: 0.2rem 0.3rem;
        font-family: monospace;
        font-weight: 600;
        text-align: center;
        white-space: nowrap;
        vertical-align: baseline;
        color: #ffffff;
        border: 1px solid #4a4444;
        background-color: #4a4444;
    }

    blockquote {
        border-left: none;
        margin: 0px;
    }

    .neo-badge {
        background-color: rgba(0, 0, 0, 0.2);
        font-size: 16px;
    }

    .neo-box {
        overflow: hidden;
        /*margin-top: -50px;
        padding-top: -100px;*/
        border-radius: 17px;
        background-color: #d9534f;
        margin-top: 3px;
        color: white;
        /*width: 325px;*/
        box-shadow: 2px 2px 2px 2px #E0E0E0;
        padding: 10px 10px;
    }

    .neo-mark {
        margin-top: -10px;
        font-weight: bold;
        font-size: 100px;
        color: white;
        font-family: "Times New Roman", Georgia, Serif;
    }

    .neo-text {
        font-size: 14px;
        /*margin-top: -65px;*/
        margin: -4px;
        text-align: center;
        height: 40px;
    }


</style>
