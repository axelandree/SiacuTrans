@using System.Web.Optimization;

@{
    Layout = null;
}

<script type="text/javascript" src="~/Areas/Transactions/Content/Lib/BloqueoF12.js"></script>
<div class="container-fluid">

    <div class="col-md-3">
        <div class="panel claro-panel-primary">
            <div class="panel-heading">
                <span class="glyphicon glyphicon-user"></span>
                <a data-toggle="collapse" href="#Datoscliente" class="collapsed transaction-ref-color">
                    Datos del Cliente
                </a>
            </div>
            <div id="Datoscliente" class="panel-body panel-collapse collapse in">
                <table class="table table-striped table-condensed">
                    <tr>
                        <td><strong>Contrato</strong></td>
                        <td><label id="lblContrato" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Customer ID</strong></td>
                        <td><label id="lblCustomerId" class="text-message-info"></label></td>                        
                    </tr>
                    <tr>
                        <td><strong>Tipo Cliente</strong></td>
                        <td><label id="lblTipoCliente" class="text-message-info"></label></td>                          
                    </tr>
                    <tr>
                        <td><strong>Cliente</strong></td>
                        <td><label id="lblCustomerName" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>DNI/RUC</strong></td>
                        <td><label id="lblIdentificationDocument" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Contacto</strong></td>
                        <td><label id="lblContact" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Representante Legal</strong></td>
                        <td><label id="lblLegalRepresentative" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Plan Actual</strong></td>
                        <td><label id="lblPlan" class="text-message-info"></label></td>                        
                    </tr>
                    <tr>
                        <td><strong>Fecha de Activación</strong></td>
                        <td><label id="lblActivationDate" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Ciclo de Facturación</strong></td>
                        <td><label id="lblCicloFacturacion" class="text-message-info"></label></td>                        
                    </tr>
                    <tr>
                        <td><strong>Límite de Crédito</strong></td>
                        <td><label id="lblLimiteCredito" class="text-message-info"></label></td>                        
                    </tr>

                </table>
            </div>


            <div id="addressDataHead" role="tab" class="panel-heading">
                <span class="glyphicon glyphicon-home"></span>
                <a data-toggle="collapse" href="#addressDataCollapse" aria-expanded="true" aria-controls="addressDataCollapse" class="collapsed transaction-ref-color"> Dirección de Facturación</a>
            </div>
            <div class="panel-body panel-collapse collapse in" id="addressDataCollapse" role="tabpanel" aria-labelledby="addressDataHead">
                <table class="table table-striped table-condensed">
                    <tr>
                        <td><strong>Dirección</strong></td>
                        <td><label id="lblDireccion" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Referencia</strong></td>
                        <td><label id="lblNotaDireccion" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>País</strong></td>
                        <td><label id="lblPais" class="text-message-info"></label></td>
                    </tr>
                    <tr>
                        <td><strong>Departamento</strong></td>
                        <td><label id="lblDepartamento" class="text-message-info"></label></td>
                    </tr> 
                    <tr>
                        <td><strong>Provincia</strong></td>
                        <td><label id="lblProvincia" class="text-message-info"></label></td>
                    </tr>                   
                    <tr>
                        <td><strong>Distrito</strong></td>
                        <td><label id="lblDistrito" class="text-message-info"></label></td>
                    </tr>                    
                    <tr id="IdlblCodPlano">
                        <td><strong>Código de Plano</strong></td>
                        <td><label id="lblCodPlano" class="text-message-info"></label></td>
                    </tr>
                    <tr id="IdlblCodUbigeo">
                        <td><strong>Código de Ubigeo</strong></td>
                        <td><label id="lblCodUbigeo" class="text-message-info"></label></td>
                    </tr>
                </table>
            </div>


        </div>
    </div>

    <div id="content" class="col-md-9 transaction-content">

        <div class="panel claro-panel-primary">
            <div class="panel-heading">
                <span class="glyphicon"></span> <strong id="header-text"></strong>
            </div>

            <div class="panel-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Desde</label>
                        <div class=" col-sm-3">
                            <div class="input-group input-group-sm">
                                <input type="text" class="form-control input-sm" id="txtStartDate" placeholder="DD/MM/AAAA">
                                <label id="calendartxtStartDate" class="btn transaction-button input-group-addon" for="txtStartDate">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </label>
                            </div>
                        </div>
                        <label class="col-sm-2 control-label">Hasta</label>
                        <div class="col-sm-3">
                            <div class="input-group input-group-sm">
                                <input type="text" class="form-control input-sm" id="txtEndDate" placeholder="DD/MM/AAAA">
                                <label id="calendartxtEndDate" class="btn transaction-button input-group-addon" for="txtEndDate">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="ddlCACDAC">Punto de Atención</label>
                        <div class="col-sm-3">
                            <select class="form-control input-sm" id="ddlCACDAC"></select>
                        </div>
                        <label class="col-sm-2 control-label">Generar OCC </label>
                        <div class="col-sm-1 checkbox" >
                            <label>
                            <input  type="checkbox" id="chkGeneraOCC" checked="checked" />
                            </label>
                        </div>
                        @*<div class="col-sm-2">
                            <input type="text" id="txtOcc" disabled="disabled" class="form-control input-sm text-right col-sm-1" /> <label class="control-label">(S/ Inc. IGV) </label>
                        </div>*@

                        <div class="col-sm-2">
                            <div class="input-group input-group-sm">                                
                                <input class="form-control text-right" id="txtOcc" style="width:55px;" type="text" disabled="disabled">
                                <span class="input-group-addon">S/ Inc. IGV</span>
                            </div>
                        </div>

                            <div class="col-sm-2">
                                <button id="btnConsult" type="button" class="btn transaction-button btn-sm"><span class="glyphicon glyphicon-search"></span> Consultar</button>
                            </div>
                        </div>
                    <div class="form-group">
                        <label class="col-sm-4 control-label" for="chkAllRecords">Mostrar todos los registros</label>
                        <div class="col-sm-1 checkbox">
                            <label>
                                <input type="checkbox" id="chkAllRecords" checked="checked">
                            </label>
                        </div>

                        <label class="col-sm-2 control-label">Enviar Correo </label>
                        <div class="col-sm-1 checkbox">
                            <label>
                                <input type="checkbox" id="chkSendMail">
                            </label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control" id="txtSendMail" placeholder="Email">
                        </div>
                    </div>
                </form>
                <div>
                    <table id="tblIncomingCalls" class="table table-bordered table-hover table-striped">
                        <thead class="btn-danger">
                            <tr>
                                <th>N°</th>
                                <th>Número</th>
                                <th>Fecha</th>
                                <th>Hora</th>
                                <th>Número Entrante</th>
                                <th>Duración</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

                <br />
                <div>
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label for="tareaNotas" class="col-sm-2 control-label">Notas </label>
                            <div class="col-sm-8">
                                <textarea class="form-control" rows="4" id="tareaNotas" onkeypress="return alpha(event)"></textarea>
                            </div>
                        </div>
                    </form>

                </div>
                <br />
                <div>
                    <center>
                        <form class="form-inline ">
                            <div class="form-group">
                                <button class="btn transaction-button btn-sm" id="btnSave" type="button"><span class="glyphicon glyphicon-floppy-disk">&nbsp;</span>Guardar</button>
                            </div>
                            <div class="form-group">
                                <button class="btn transaction-button btn-sm" id="btnPrint" type="button"><span class="glyphicon glyphicon-print">&nbsp;</span>Imprimir</button>
                            </div>
                            <div class="form-group">
                                <button class="btn transaction-button btn-sm" id="btnExport" type="button"><span class="glyphicon glyphicon-export">&nbsp;</span>Exportar</button>
                            </div>
                            <div class="form-group" id="loo">
                                <button class="btn transaction-button btn-sm" id="btnConstancy" type="button"><span class="glyphicon glyphicon-file">&nbsp;</span>Constancia</button>
                            </div>
                            <div class="form-group">
                                <button class="btn transaction-button btn-sm" id="btnClose" type="button"><span class="glyphicon glyphicon-remove">&nbsp;</span>Cerrar</button>
                            </div>
                        </form>
                    </center>

                </div>
                <br />
                <br />
                <br />
                <br />
            </div>
        </div>
    </div>
</div>



@Scripts.Render("~/bundles/Transactions/Hfc/HfcIncomingCallDetail")